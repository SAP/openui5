{
	"name": "openui5",
	"version": "2.0.0-SNAPSHOT",
	"description": "OpenUI5 Core & Runtime",
	"private": true,
	"scripts": {
		"start": "npm run start --workspace src/testsuite --",
		"testsuite": "npm run testsuite --workspace src/testsuite",
		"sdk": "npm run sdk --workspace src/testsuite",
		"build-sdk": "npm run build-sdk --workspace src/testsuite",
		"serve-sdk": "npm run serve-sdk --workspace src/testsuite",
		"test": "npm run lint",
		"lint": "eslint ./src --quiet --cache",
		"clean": "npm x --ws -c 'rimraf --verbose ./dist'",
		"link-all": "wsrun --serial link",
		"unlink-all": "wsrun --serial unlink",
		"karma": "karma start lib/test/karma.conf.js",
		"karma-all": "run-s -c \"karma:*\"",
		"karma-coverage-all": "run-s -c \"karma:* -- --coverage\"",
		"karma-ci-coverage-all": "run-s -c \"karma:* -- --coverage --ci\"",
		"karma:sap.f": "npm run karma -- --lib=sap.f",
		"karma:sap.m": "npm run karma -- --lib=sap.m",
		"karma:sap.tnt": "npm run karma -- --lib=sap.tnt",
		"karma:sap.ui.codeeditor": "npm run karma -- --lib=sap.ui.codeeditor",
		"karma:sap.ui.core": "npm run karma -- --lib=sap.ui.core",
		"karma:sap.ui.documentation": "npm run karma -- --lib=sap.ui.documentation",
		"karma:sap.ui.dt": "npm run karma -- --lib=sap.ui.dt",
		"karma:sap.ui.fl": "npm run karma -- --lib=sap.ui.fl",
		"karma:sap.ui.mdc": "npm run karma -- --lib=sap.ui.mdc",
		"karma:sap.ui.integration": "npm run karma -- --lib=sap.ui.integration",
		"karma:sap.ui.layout": "npm run karma -- --lib=sap.ui.layout",
		"karma:sap.ui.rta": "npm run karma -- --lib=sap.ui.rta",
		"karma:sap.ui.suite": "npm run karma -- --lib=sap.ui.suite",
		"karma:sap.ui.support": "npm run karma -- --lib=sap.ui.support",
		"karma:sap.ui.testrecorder": "npm run karma -- --lib=sap.ui.testrecorder",
		"karma:sap.ui.table": "npm run karma -- --lib=sap.ui.table",
		"karma:sap.ui.unified": "npm run karma -- --lib=sap.ui.unified",
		"karma:sap.uxap": "npm run karma -- --lib=sap.uxap",
		"generate-cldr": "cd lib/cldr-openui5 && npm start"
	},
	"keywords": [
		"openui5",
		"sapui5",
		"ui5"
	],
	"author": "SAP SE",
	"contributors": [
		"SAP SE <*@sap.com>"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/SAP/openui5.git"
	},
	"engines": {
		"node": "^20.11.0 || >=22.0.0",
		"npm": ">= 8"
	},
	"devDependencies": {
		"@ui5/builder": "^4.0.2",
		"@ui5/cli": "^4.0.5",
		"@ui5/project": "^4.0.2",
		"@ui5/server": "^4.0.3",
		"@ui5/tooling-webc": "^0.2.14",
		"@ui5/webcomponents": "1.18.0",
		"@ui5/webcomponents-base": "1.18.0",
		"@ui5/webcomponents-fiori": "1.18.0",
		"@ui5/webcomponents-icons": "1.18.0",
		"@ui5/webcomponents-icons-business-suite": "1.18.0",
		"@ui5/webcomponents-icons-tnt": "1.18.0",
		"@ui5/webcomponents-localization": "1.18.0",
		"@ui5/webcomponents-theming": "1.18.0",
		"async": "^3.2.4",
		"cheerio": "1.0.0",
		"cookie": "^0.4.2",
		"decompress": "^4.2.1",
		"eslint": "^8.56.0",
		"glob": "^7.2.3",
		"http-proxy": "^1.18.1",
		"karma": "^6.4.2",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage": "^2.2.1",
		"karma-junit-reporter": "^2.0.1",
		"karma-ui5": "^3.0.4",
		"lit-html": "2.2.2",
		"moment": "^2.29.4",
		"node-fetch": "^3.2.10",
		"npm-run-all": "^4.1.5",
		"pacote": "^15.1.1",
		"rimraf": "^4.3.1",
		"semver": "^7.5.4",
		"set-cookie-parser": "^2.5.1",
		"wsrun": "^5.2.0"
	},
	"overrides": {
		"grunt-jsdoc": {
			"jsdoc": "^4.0.2"
		}
	},
	"workspaces": [
		"src/*"
	]
}