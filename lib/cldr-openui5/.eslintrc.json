{
	"env": {
		"node": true,
		"browser": false
	},
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"indent": [
			"error",
			"tab",
			{"SwitchCase": 1}
		],
		"max-len": [
			"error",
			{"code": 120, "ignoreUrls": true, "ignoreRegExpLiterals": true}
		],
		"no-console": "off",
		"no-prototype-builtins": "off",
		"no-var": "error",
		"quotes": [
			"error",
			"double",
			{"allowTemplateLiterals": true}
		],
		"strict": [
			"error",
			"never"
		]
	}
}
