{
	"source": {
		"excludePattern": "(/|\\\\)library-all\\.js|(/|\\\\).*-preload\\.js|^jquery-.*\\.js|^sap-.*\\.js"
	},
	"opts" : {
		"recurse": true,
		"template" : "lib/jsdoc/ui5/template"
	},
	"plugins": [
		"lib/jsdoc/ui5/plugin.js"
	],
	"templates" : {
		"ui5" : {
			"variants": [
				"apijson"
			],
			"version": "${version}",
            "uilib": "${libraryName}",
			"apiJsonFolder": "${apiJsonFolder}",
			"apiJsonFile": "${apiJsonFile}",
			"includeSettingsInConstructor": false
		}
	}
}
