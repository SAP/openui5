<?xml version="1.0" encoding="UTF-8" ?>
<library xmlns="http://www.sap.com/sap.ui.library.xsd">

	<name>sap.ui.webc.common</name>
	<vendor>SAP SE</vendor>
	<version>${version}</version>
	<copyright>${copyright}</copyright>

	<documentation>UI5 Web Components Retrofit Common</documentation>

	<dependencies>
		<dependency>
			<libraryName>sap.ui.core</libraryName>
		</dependency>
	</dependencies>

	<appData>
		<!-- excludes for the JSDoc -->
		<jsdoc xmlns="http://www.sap.com/ui5/buildext/jsdoc">
			<exclude name="sap.ui.webc.common.thirdparty."/>
		</jsdoc>
		<!-- excludes for the JSCoverage -->
		<jscoverage xmlns="http://www.sap.com/ui5/buildext/jscoverage">
			<exclude name="sap.ui.webc.common.thirdparty."/>
		</jscoverage>
		<!-- Thirdparty references -->
		<thirdparty xmlns="http://www.sap.com/ui5/buildext/thirdparty" >
			<lib name="ui5-web-components" displayName="UI5 Web Components" npmName="@ui5/webcomponents" version="1.18.0" homepage="https://sap.github.io/ui5-webcomponents/">
				<license url="https://github.com/SAP/ui5-webcomponents/blob/master/LICENSE.txt" type="Apache-2.0"></license>
				<copyright>SAP</copyright>
				<pattern>sap/ui/webc/common/thirdparty/base/*.*</pattern>
				<pattern>sap/ui/webc/common/thirdparty/theming/*.*</pattern>
				<pattern>sap/ui/webc/common/thirdparty/localization/*.*</pattern>
				<pattern>sap/ui/webc/common/thirdparty/icons/*.*</pattern>
				<pattern>sap/ui/webc/common/thirdparty/icons-tnt/*.*</pattern>
				<pattern>sap/ui/webc/common/thirdparty/icons-business-suite/*.*</pattern>
			</lib>
			<lib name="lit-html" displayName="lit-html" npmName="lit-html" version="2.2.2" hash="94bafc7d74b032d0b543afb10cae9b08" homepage="https://lit-html.polymer-project.org">
				<license url="https://github.com/lit/lit/blob/main/LICENSE" type="BSD-3-Clause"></license>
				<copyright>Google LLC</copyright>
				<pattern>sap/ui/webc/common/thirdparty/lit-html/*.*</pattern>
			</lib>
		</thirdparty>


		<releasenotes xmlns="http://www.sap.com/ui5/buildext/releasenotes"
						url="../../../../../test-resources/sap/ui/webc/common/relnotes/changes-{major}.{minor}.json"
						resolve="lib" />
		<ownership xmlns="http://www.sap.com/ui5/buildext/ownership">
		   <component>CA-UI5-CTR</component><!-- default component for library, embedded text as a shortcut for <name>text</name> -->
		</ownership>
	</appData>
</library>
