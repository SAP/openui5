{
	"_version": "1.15.0",
	"sap.app": {
		"id": "card.explorer.filter.agenda.card",
		"type": "card",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"shortTitle": "A short title for this Card",
		"info": "Additional information about this Card",
		"description": "A long description for this Card"
	},
	"sap.card": {
		"type": "List",
		"configuration": {
			"filters": {
				"dateTimeRange": {
					"type": "DateRange",
					"value": {
						"option": "dateTimeRange",
						"values": [
							"2023-02-07T10:00:00.000",
							"2023-02-07T17:45:00.000"
						]
					},
					"options": [
						"today",
						"dateTime",
						"dateTimeRange",
						"fromDateTime",
						"toDateTime"
					],
					"label": "Activity period"
				}
			}
		},
		"header": {
			"title": "Agenda"
		},
		"content": {
			"data": {
				"request": {
					"url": "/agenda",
					"parameters": {
						"startDate": "{filters>/dateTimeRange/range/start}",
						"endDate": "{filters>/dateTimeRange/range/end}"
					}
				}
			},
			"item": {
				"description": "{= format.dateTime(${Time}, {format: 'yMMMdHHmm'}) }",
				"title": {
					"value": "{Title}"
				},
				"icon": {
					"src": "{Icon}"
				}
			}
		}
	}
}