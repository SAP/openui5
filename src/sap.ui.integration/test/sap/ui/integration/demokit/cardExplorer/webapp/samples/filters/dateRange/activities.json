{
	"_version": "1.15.0",
	"sap.app": {
		"id": "card.explorer.activities.timeline.card",
		"type": "card",
		"title": "Sample of a Activities Timeline",
		"subTitle": "Sample of a Activities Timeline",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"shortTitle": "A short title for this Card",
		"info": "Additional information about this Card",
		"description": "A long description for this Card",
		"tags": {
			"keywords": [
				"Timeline",
				"Card",
				"Sample"
			]
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://activity-items"
		}
	},
	"sap.card": {
		"type": "Timeline",
		"configuration": {
			"filters": {
				"activityPeriod": {
					"type": "DateRange",
					"value": {
						"option": "today",
						"values": []
					},
					"options": [
						"today",
						"yesterday",
						"tomorrow",
						"dateRange",
						"nextDays",
						"lastDays"
					],
					"label": "Activity period"
				}
			}
		},
		"header": {
			"title": "My Activities"
		},
		"content": {
			"data": {
				"request": {
					"url": "/activities",
					"parameters": {
						"startDate": "{filters>/activityPeriod/range/start}",
						"endDate": "{filters>/activityPeriod/range/end}"
					}
				}
			},
			"item": {
				"dateTime": {
					"value": "{Time}"
				},
				"description": {
					"value": "{Description}"
				},
				"title": {
					"value": "{Title}"
				},
				"icon": {
					"src": "{Icon}"
				}
			}
		}
	}
}