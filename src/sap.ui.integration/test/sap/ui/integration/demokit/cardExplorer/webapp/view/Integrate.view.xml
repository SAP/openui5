<mvc:View
	controllerName="sap.ui.demo.cardExplorer.controller.Integrate"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Page class="topicPage" showHeader="true" title="{/pageTitle}">
		<content>
			<core:HTML id="testHTML"
					   preferDOM="false"
					   content='&lt;div style="height:100%; overflow-y: hidden; overflow-x:auto;"&gt;
										&lt;iframe {/iframeAttribute} class="sapUiTopicsIframe" name="content" src="{/topicURL}" frameborder="0""&gt;&lt;/iframe&gt;
								&lt;/div&gt;'>
			</core:HTML>
		</content>
	</Page>
</mvc:View>
