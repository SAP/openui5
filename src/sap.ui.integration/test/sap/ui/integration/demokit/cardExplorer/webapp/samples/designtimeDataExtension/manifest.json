{
	"sap.app": {
		"type": "card",
		"id": "card.explorer.extension.getData"
	},
	"sap.card": {
		"extension": "./DataExtension",
		"configuration": {
			"destinations": {
				"Northwind_V3": {
					"name": "Northwind_V3",
					"label": "Northwind V3 Service URL",
					"defaultUrl": "https://services.odata.org/V3/Northwind/Northwind.svc"
				}
			},
			"parameters": {
				"maxItems": {
					"value": 4
				},
				"stringWithRequestExtensionList": {
					"value": "Quality Management"
				},
				"stringWithDataFromExtensionList": {
					"value": "Quality Management"
				}
			}
		},
		"data": {
			"extension": {
				"method": "getData"
			}
		},
		"type": "List",
		"designtime": "dt/designtime",
		"header": {
			"title": "Available Trainings",
			"icon": {
				"src": "sap-icon://activities"
			}
		},
		"content": {
			"item": {
				"title": "{title}",
				"description": "Trainer: {trainer}",
				"info": {
					"value": "Location: {location}"
				}
			},
			"maxItems": "{{parameters.maxItems}}"
		}
	}
}