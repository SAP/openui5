{
	"_version": "2.0.0",
	"sap.app": {
		"id": "my.component.sample.Translation",
		"type": "card",
		"i18n": "i18n/i18n.properties",
		"title": "Sample of a Component Content",
		"subTitle": "Sample of a Component Content",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"shortTitle": "A short title for this Card",
		"info": "Additional information about this Card",
		"description": "A long description for this Card",
		"tags": {
			"keywords": [
				"Component",
				"Card",
				"Sample"
			]
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://technical-object"
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "my.component.sample.Translation.View",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.38",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "my.component.sample.Translation.i18n.i18n",
					"supportedLocales": [
						"",
						"en"
					],
					"fallbackLocale": ""
				}
			}
		}
	},
	"sap.card": {
		"type": "Component",
		"header": {
			"title": "{i18n>title}"
		},
		"content": {
			"minHeight": "10rem"
		}
	}
}