<mvc:View
	controllerName="sap.ui.integration.sample.BlockingMessage.Main"
	displayBlock="true"
	xmlns:w="sap.ui.integration.widgets"
	xmlns:f="sap.f"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<App>
		<Page showHeader="false" class="sapUiContentPadding" >
			<f:GridContainer>
				<w:Card id="card1" manifest="./cards/listCard.json">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="3" />
					</w:layoutData>
				</w:Card>
				<w:Card id="card2" manifest="./cards/listCardEmpty.json">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="3" />
					</w:layoutData>
				</w:Card>
			</f:GridContainer>
		</Page>
	</App>
</mvc:View>
