<mvc:View
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	width="100%"
	height="100%"
	displayBlock="true"
	controllerName="my.component.sample.requestData.Main">
		<List
			items="{products>/}"
			showSeparators="Inner"
			class="sapUiSmallMarginBottom"
		>
			<StandardListItem title="{products>Name}"></StandardListItem>
		</List>
</mvc:View>