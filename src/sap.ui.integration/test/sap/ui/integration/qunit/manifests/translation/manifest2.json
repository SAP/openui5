		"type": "List",
		"header": {
			"title": "{{TITLE}}",
			"subTitle": "{{SUBTITLE}}",
			"icon": {
				"src": "sap-icon://business-objects-experience"
			},
			"status": {
				"text": {
					"format": {
						"translationKey": "i18n>COUNT_X_OF_Y",
						"parts": [
							"parameters>/visibleItems",
							"/count"
						]
					}
				}
			}
		},
		"content": {
			"data": {
				"path": "/items"
			},
			"maxItems": 2,
			"item": {
				"icon": {
					"src": "{icon}"
				},
				"title": {
					"value": "{title}"
				},
				"description": {
					"value": "{description}"
				},
				"highlight": "{highlight}",
				"info": {
					"state": "{infoState}"
				}
			}
		}
	}
}