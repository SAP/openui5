{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.my.test.widget.component",
		"type": "widget",
		"i18n": {
		  "bundleUrl": "i18n/i18n.properties",
		  "supportedLocales": [
		    "",
		    "en"
		  ],
		  "fallbackLocale": "en"
		},
        "title": "{{TITLE}}",
        "description": "{{DESCRIPTION}}"
	},
	"sap.ui": {
		"technology": "UI5"
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.78.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"rootView": {
			"viewName": "sap.my.test.widget.component.view.Widget",
			"type": "XML",
			"async": true,
			"id": "app"
		}
	},
	"sap.widget": {
		"configuration": {
			"parameters": {
				"title": {
					"value": "{{TITLE}}"
				},
				"description": {
					"value": "{{DESCRIPTION}}"
				}
			}
		}
	}
}
