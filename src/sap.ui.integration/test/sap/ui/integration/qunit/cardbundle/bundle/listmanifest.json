{
	"sap.app": {
		"id": "my.test.card.list",
		"type": "card",
		"i18n": "i18n/i18n.properties"
	},
	"sap.card": {
		"type": "List",
		"header": {
			"title": "{{appTitle}}",
			"icon": {
				"src": "//icons/edit.png"
			}
		},
		"content": {
			"data": {
				"json": [
					{
						"Icon": "./icons/edit.png"
					}
				]
			},
			"item": {
				"icon": {
					"src": "{Icon}"
				}
			}
		}
	}
}