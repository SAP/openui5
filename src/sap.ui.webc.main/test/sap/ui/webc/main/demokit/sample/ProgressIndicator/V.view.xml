<mvc:View controllerName="sap.ui.webc.main.sample.ProgressIndicator.C" xmlns="sap.ui.webc.main" xmlns:fiori="sap.ui.webc.fiori" xmlns:mvc="sap.ui.core.mvc" height="100%">

	<fiori:Page>
		<fiori:header>
			<fiori:Bar>
				<fiori:middleContent>
					<Title text="Progress Indicator"></Title>
				</fiori:middleContent>
			</fiori:Bar>
		</fiori:header>

		<fiori:content>

			<Title level="H3"
				text="Basic Progress Indicator"
				class="sapUiTinyMarginBottom sapUiTinyMarginTop"
				wrappingType="Normal"/>
			<ProgressIndicator value="0" />
			<ProgressIndicator value="25" />
			<ProgressIndicator value="75" disabled="true" />
			<ProgressIndicator value="100" />

			<Title level="H3"
				text="Progress Indicator With Custom Display Value"
				class="sapUiMediumMarginTop sapUiTinyMarginBottom"
				wrappingType="Normal"/>
			<ProgressIndicator value="25" displayValue="Custom Display Value" />

			<Title level="H3"
				text="Progress Indicator With Value State"
				class="sapUiMediumMarginTop sapUiTinyMarginBottom"
				wrappingType="Normal"/>
			<ProgressIndicator valueState="None" value="25" />
			<ProgressIndicator valueState="Error" value="50" />
			<ProgressIndicator valueState="Warning" value="75" />
			<ProgressIndicator valueState="Success" value="90" />
			<ProgressIndicator valueState="Information" value="100" />

		</fiori:content>
	</fiori:Page>
</mvc:View>