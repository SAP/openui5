<mvc:View controllerName="sap.ui.webc.main.sample.RatingIndicator.C" xmlns:l="sap.ui.layout" xmlns="sap.ui.webc.main" xmlns:fiori="sap.ui.webc.fiori" xmlns:mvc="sap.ui.core.mvc" height="100%">
	<l:VerticalLayout class="sapUiContentPadding" width="100%">
		<l:content>
			<Toast id="demoToast" duration="2000"></Toast>
			<Label text="Rating Indicator"/>
			<RatingIndicator accessibleName="Rating Indicator" change="handleChange"></RatingIndicator>
			<Label text="Rating Indicator with predefined value"/>
			<RatingIndicator accessibleName="Rating Indicator with predefined value" value="3" change="handleChange"></RatingIndicator>
			<Label text="Rating Indicator with max value"/>
			<RatingIndicator accessibleName="Rating Indicator with max value" max="10" change="handleChange"></RatingIndicator>
			<Label text="Readonly Rating Indicator"/>
			<RatingIndicator accessibleName="Readonly Rating Indicator" readonly="true"></RatingIndicator>
			<Label text="Disabled Rating Indicator"/>
			<RatingIndicator accessibleName="Disabled Rating Indicator" enabled="false" value="3"></RatingIndicator>
		</l:content>
	</l:VerticalLayout>
</mvc:View>