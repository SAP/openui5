<mvc:View
	height="100%"
	controllerName="sap.m.sample.ToolbarEnabled.Toolbar"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<core:InvisibleText id="text1" text="Label text"/>

	<CheckBox text="Enabled" selected="true" select="onCheckBoxSelect"/>

	<OverflowToolbar id="toolbar">
		<Button text="Accept" type="Accept"/>
		<ToolbarSpacer/>
		<CheckBox text="CheckBox"/>
		<ToolbarSpacer/>
		<Input ariaLabelledBy="text1" width="100px" value="Input"/>
		<ToolbarSpacer/>
		<RadioButton text="RadioButton"/>
		<ToolbarSpacer/>
		<Button text="Reject" type="Reject"/>
	</OverflowToolbar>

</mvc:View>