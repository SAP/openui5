<mvc:View
	controllerName="sap.m.sample.QuickViewCard.C"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	height="100%">
	<Page
		showHeader="false"
		showNavButton="false"
		class="sapUiContentPadding">
		<Button
			id="buttonBack"
			enabled="false"
			text="Navigate Back"
			press=".onButtonBackClick"
			class="sapUiSmallMarginBottom" />
		<core:Fragment fragmentName="sap.m.sample.QuickViewCard.QuickViewCard" type="XML" />
	</Page>
</mvc:View>