{
	"_version": "2.0.0",
	"sap.app": {
		"id": "sap.m.sample.IllustratedMessageInPageCustom",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"type": "application",
		"title": "Application 'IllustratedMessageInPageCustom'"
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": "sap.m.sample.IllustratedMessageInPageCustom.view.Main",
		"resourceRoots": {
			"illustrations": "custom_illustrations"
		},
		"dependencies": {
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"config": {
			"sample": {
				"stretch": true,
				"files": [
					"custom_illustrations/metadata.json",
					"custom_illustrations/Custom-Dialog-EmptyCart.svg",
					"custom_illustrations/Custom-Dot-EmptyCart.svg",
					"custom_illustrations/Custom-Scene-EmptyCart.svg",
					"custom_illustrations/Custom-Spot-EmptyCart.svg",
					"controller/Main.controller.js",
					"view/Main.view.xml",
					"Component.js",
					"manifest.json"
				]
			}
		}
	}
}