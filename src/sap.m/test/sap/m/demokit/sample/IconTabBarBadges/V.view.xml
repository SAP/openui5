				</IconTabFilter>
				<IconTabFilter
					icon="sap-icon://compare"
					iconColor="Critical"
					count="51"
					text="Heavy"
					key="Heavy">
					<customData>
						<BadgeCustomData visible="true"/>
					</customData>
				</IconTabFilter>
				<IconTabFilter
					icon="sap-icon://inventory"
					iconColor="Negative"
					count="19"
					text="Overweight"
					key="Overweight">
					<customData>
						<BadgeCustomData visible="true"/>
					</customData>
				</IconTabFilter>
			</items>
		</IconTabBar>

		<IconTabBar id="iconTabBar8" class="sapUiResponsiveContentPadding">
		<items>
			<IconTabFilter key="info" text="Info">
				<items>
					<IconTabFilter text="Info one">
						<Text text="Info one content goes here..." />
					</IconTabFilter>
					<IconTabFilter text="Info two">
						<Text text="Info two content goes here..." />
						<customData>
							<BadgeCustomData visible="true"/>
						</customData>
					</IconTabFilter>
					<IconTabFilter text="Info three with badge">
						<Text text="Info three content goes here..." />
						<customData>
							<BadgeCustomData visible="true"/>
						</customData>
					</IconTabFilter>
					<IconTabFilter text="Info four">
						<Text text="Info four content goes here..." />
					</IconTabFilter>
				</items>
			</IconTabFilter>

			<IconTabFilter key="attachments" text="Attachments">
				<customData>
					<BadgeCustomData visible="true"/>
				</customData>
				<items>
					<IconTabFilter text="Attachment one">
						<Text text="Attachment one goes here..." />
					</IconTabFilter>
					<IconTabFilter text="Attachment two">
						<Text text="Attachment two goes here..." />
					</IconTabFilter>
				</items>

				<Text text="Attachments own content goes here..." />
			</IconTabFilter>

			<IconTabFilter key="notes" text="Notes">
				<customData>
					<BadgeCustomData visible="true"/>
				</customData>
				<items>
					<IconTabFilter text="Note one">
						<Text text="Note one goes here..." />
					</IconTabFilter>
					<IconTabFilter text="Note two">
						<customData>
							<BadgeCustomData visible="true"/>
						</customData>
						<Text text="Note two goes here..." />
					</IconTabFilter>
				</items>

				<Text text="Notes own content goes here..." />
			</IconTabFilter>
		</items>
	</IconTabBar>
	</Panel>
</mvc:View>