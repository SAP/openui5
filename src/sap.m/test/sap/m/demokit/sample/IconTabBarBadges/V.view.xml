				<IconTabFilter
					icon="sap-icon://begin"
					iconColor="Positive"
					design="Horizontal"
					count="53 of 123"
					text="Confirm Ok"
					key="Ok" >
					<Text text="Filtered items goes here ..." />
				</IconTabFilter>
				<IconTabSeparator icon="sap-icon://open-command-field" />
				<IconTabFilter
					icon="sap-icon://compare"
					iconColor="Critical"
					design="Horizontal"
					count="51 of 123"
					text="Check Heavys"
					key="Heavy">
					<customData>
						<BadgeCustomData visible="true"/>
					</customData>
				</IconTabFilter>
				<IconTabSeparator icon="sap-icon://open-command-field" />
				<IconTabFilter
					icon="sap-icon://inventory"
					iconColor="Negative"
					design="Horizontal"
					count="19 of 123"
					text="Claim Overweights"
					key="Overweight" />
			</items>
		</IconTabBar>

		<IconTabBar id="iconTabBar7" class="sapUiResponsiveContentPadding">
			<items>
				<IconTabFilter
					showAll="true"
					count="123"
					text="Products"
					key="All" >
					<Text text="Filtered items goes here ..." />
					<customData>
						<BadgeCustomData visible="true"/>
					</customData>
				</IconTabFilter>
				<IconTabSeparator />
				<IconTabFilter
					icon="sap-icon://begin"
					iconColor="Positive"
					count="53"
					text="Ok"
					key="Ok">
					<customData>
						<BadgeCustomData visible="true"/>
