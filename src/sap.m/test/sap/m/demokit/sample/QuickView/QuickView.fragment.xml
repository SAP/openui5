<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<QuickView id="quickView" pages="{/pages}">
		<QuickViewPage
				pageId="{pageId}"
				header="{header}"
				icon="{icon}"
				title="{title}"
				titleUrl="{titleUrl}"
				description="{description}"
				groups="{groups}">
			<QuickViewGroup heading="{heading}" elements="{elements}">
				<QuickViewGroupElement
						label="{label}"
						value="{value}"
						url="{url}"
						type="{elementType}"
						pageLinkId="{pageLinkId}"
						emailSubject="{emailSubject}"
						target="{target}"
						>
				</QuickViewGroupElement>
			</QuickViewGroup>
		</QuickViewPage>
	</QuickView>
</core:FragmentDefinition>
