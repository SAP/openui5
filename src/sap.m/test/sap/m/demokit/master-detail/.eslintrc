{
	"env": {
		"browser": true
	},
	"globals": {
		"sap": true,
		"jQuery": true,
		"ok": true,
		"strictEqual": true,
		"module": true,
		"QUnit": true,
		"sinon": true
	},
	"plugins": [
		"eslint-plugin-openui5"
	],
	"rules": {
	  	"block-scoped-var": 1,
	  	"brace-style": [2, "1tbs", { "allowSingleLine": true }],
	  	"camelcase": 1,
	  	"comma-spacing": 0,
	  	"complexity":	[0,11],
	  	"consistent-return": 1,
	  	"consistent-this": 2,
	  	"curly": [2,"all"],
	  	"default-case": 1,
	  	"dot-notation": 1,
	  	"eol-last": 0,
	  	"eqeqeq":	1,
	  	"global-strict": 2,
	  	"global-strict": 2,
	  	"guard-for-in": 1,
	  	"key-spacing": [0,{"afterColon":true,"beforeColon":false}],
	  	"line-endings": 0,
	  	"max-depth": [0,4],
		"max-len": [0,80,4],
		"max-nested-callbacks": [1, 3],
		"max-params":		[0,3],
		"max-statements":		[0,10],
		"new-cap": [1,{"capIsNew":false}],
		"new-parens": 2,
		"no-alert": 2,
		"no-array-constructor": 2,
		"no-catch-shadow": 2,
		"no-comma-dangle": 1,
		"no-delete-var": 2,
		"no-div-regex": 2,
		"no-empty": 2,
		"no-empty-label": 2,
		"no-eq-null": 1,
		"no-extend-native": 2,
		"no-extra-bind": 2,
		"no-extra-boolean-cast": 1,
		"no-extra-semi": 2,
		"no-extra-strict": 2,
		"no-fallthrough":	2,
		"no-floating-decimal": 2,
		"no-implied-eval": 2,
		"no-irregular-whitespace": 1,
		"no-iterator": 2,
		"no-label-var": 2,
		"no-labels": 2,
		"no-lone-blocks":	2,
		"no-lonely-if": 1,
		"no-loop-func": 2,
		"no-mixed-spaces-and-tabs": [2, true],
		"no-multi-spaces": 1,
		"no-multi-str":	2,
		"no-nested-ternary": 2,
		"no-new": 1,
		"no-new-object":	2,
		"no-new-wrappers": 1,
		"no-octal": 2,
		"no-octal-escape": 2,
		"no-process-exit": 2,
		"no-proto": 2,
		"no-redeclare": 1,
		"no-return-assign": 2,
		"no-script-url": 2,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-shadow": 1,
		"no-shadow-restricted-names":	2,
		"no-space-before-semi": 1,
		"no-spaced-func":	2,
		"no-trailing-spaces": 0,
		"no-undef-init": 2,
		"no-underscore-dangle": 0,
		"no-unreachable":	2,
		"no-unused-expressions":	2,
		"no-unused-vars": [2, {"vars":"all", "args":"none"}],
		"no-use-before-define": [1, "nofunc"],
		"no-warning-comments": [1,{"location":"start","terms":["todo","fixme","xxx"]}],
		"no-wrap-func": 2,
		"openui5/line-endings": 2,
		"quotes": [1,"double","avoid-escape"],
		"radix": 2,
		"space-after-keywords": [2, "always"],
		"space-infix-ops": 2,
		"space-return-throw-case": 2,
		"space-unary-ops": 2,
		"space-unary-word-ops": 0,
		"strict": 1,
		"valid-jsdoc": [1, {"requireReturn": false}],
		"wrap-iife": [2, "any"],
		"yoda":	[2,"never"]
	}
}
