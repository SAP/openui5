<mvc:View
	controllerName="sap.ui.demo.masterdetail.controller.DetailObjectNotFound"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

	<MessagePage
		id="page"
		title="{i18n>detailTitle}"
		text="{i18n>noObjectFoundText}"
		icon="sap-icon://product"
		description=""
		showNavButton="{=
			${device>/system/phone} ||
			${device>/system/tablet} &amp;&amp;
			${device>/orientation/portrait}
		}"
		navButtonPress=".onNavBack">
	</MessagePage>

</mvc:View>