{
	"_version": "2.0.2",
	"sap.app": {
		"id": "sap.m.sample.buttonShortcut",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "Application 'buttonShortcut'"
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": "sap.m.sample.buttonShortcut.View",
		"dependencies": {
			"libs": {
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"commands": {
			"Save": {
				"shortcut": "Ctrl+S"
			},
			"Delete": {
				"shortcut": "Ctrl+D"
			}
		}
	}
}