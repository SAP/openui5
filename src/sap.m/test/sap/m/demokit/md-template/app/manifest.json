{
	"_version": "1.1.0",

	"sap.app": {
		"_version": "1.1.0",
		"id": "sap.ui.demo.mdtemplate",
		"type": "application",
		
		"i18n": "i18n/i18n.properties",
		"title": "{i18n>appTitle}",
		"description": "{i18n>appDescription}",
		
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "CA-UI5-DOC"
	},


	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		
		"icon": "sap-icon://detail-view",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},


	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": "sap.ui.demo.mdtemplate.view.App",

		"dependencies": {
			"minUI5Version": "1.28.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},

		"config": {
			"serviceUrl": "here/goes/your/serviceUrl/"
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.mdtemplate.i18n.i18n"
				}
			}
		},

		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sap.ui.demo.mdtemplate.view",
				"controlId": "idAppControl",
				"controlAggregation": "detailPages",
				"bypassed": {
					"target": ["master", "notFound"]
				}
			},

			"routes": [
				{
					"pattern": "",
					"name": "master",
					"target": ["object", "master"]
				},
				{
					"pattern": "object/{objectId}",
					"name": "object",
					"target": ["master", "object"]
				},
				{
					"pattern": "object/{objectId}/lineitem/{lineItemId}",
					"name": "lineItem",
					"target": ["master", "lineItem"]
				}
			],

			"targets": {
				"master": {
					"viewName": "Master",
					"viewLevel": 1,
					"controlAggregation": "masterPages"
				},
				"object": {
					"viewName": "Detail",
					"viewLevel": 2
				},
				"lineItem": {
					"viewName": "LineItem",
					"viewLevel": 3
				},
				"detailObjectNotFound": {
					"viewName": "DetailObjectNotFound",
					"viewLevel": 3
				},
				"detailNoObjectsAvailable": {
					"viewName": "DetailNoObjectsAvailable",
					"viewLevel": 3
				},
				"lineItemNotFound": {
					"viewName": "LineItemNotFound",
					"viewLevel": 3
				},
				"notFound": {
					"viewName": "NotFound",
					"viewLevel": 3
				}
			}
		}
	}
}
