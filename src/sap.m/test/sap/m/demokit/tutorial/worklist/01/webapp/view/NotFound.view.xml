<mvc:View
	controllerName="mycompany.myapp.controller.NotFound"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<MessagePage
		description=""
		icon="{sap-icon://document}"
		id="page"
		text="{i18n>notFoundText}"
		title="{i18n>notFoundTitle}">
		<customDescription>
			<Link
				id="link"
				press="onLinkPressed"
				text="{i18n>backToWorklist}"/>
		</customDescription>
	</MessagePage>
</mvc:View>
