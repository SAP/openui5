<mvc:View
	controllerName="mycompany.myapp.controller.NotFound"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<MessagePage
		id="page"
		description=""
		icon="{sap-icon://product}"
		text="{i18n>noObjectFoundText}"
		title="{i18n>objectTitle}">
		<customDescription>
			<Link
				id="link"
				press="onLinkPressed"
				text="{i18n>backToWorklist}"/>
		</customDescription>
	</MessagePage>
</mvc:View>
