<mvc:View
	controllerName="sap.ui.demo.cart.controller.App"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc">
	<App
		id="app"
		class="sapUiDemoCart">
		<f:FlexibleColumnLayout
			id="layout"
			busy="{appView>/busy}"
			busyIndicatorDelay="{appView>/delay}"
			layout="{appView>/layout}"
			backgroundDesign="Translucent"
			class="sapUiDemoCart"
			stateChange=".onStateChange">
		</f:FlexibleColumnLayout>
	</App>
</mvc:View>