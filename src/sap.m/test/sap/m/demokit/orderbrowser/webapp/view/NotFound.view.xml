<mvc:View
	controllerName="sap.ui.demo.orderbrowser.controller.NotFound"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<MessagePage
		id="page"
		title="{i18n>notFoundTitle}"
		text="{i18n>notFoundText}"
		icon="sap-icon://document"
		showNavButton="true"
		navButtonPress=".onNavBack">
	</MessagePage>
</mvc:View>