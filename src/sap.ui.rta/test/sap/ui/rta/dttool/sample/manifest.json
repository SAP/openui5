{
	"_version": "1.8.0",

	"sap.app": {
		"id": "sap.ui.rta.dttool.sample",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": false
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.rta.dttool.sample.Main",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"handleValidation": true,
		"dependencies": {
			"minUI5Version": "${project.version}",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.dt": {},
				"sap.ui.rta": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties",
				"async": true
			}
		},
		"resources": {
			"css": [{
				"uri": "../css/style.css"
			}]
		}
	}
}
