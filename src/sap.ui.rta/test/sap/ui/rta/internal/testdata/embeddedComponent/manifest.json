{
	"_version": "1.8.0",

	"sap.app": {
		"id": "sap.ui.rta.test.embeddedComponent",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "CA-UI5-FL-RTA"
	},

	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": false
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.rta.test.embeddedComponent.view.Root",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"handleValidation": true,
		"dependencies": {
			"minUI5Version": "${project.version}",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.dt": {},
				"sap.ui.rta": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties",
				"async": true
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "sap.ui.rta.test.embeddedComponent.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": [
						"main"
					]
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "main",
					"target": [
						"main"
					]
				}
			],
			"targets": {
				"main": {
					"viewName": "Main",
					"viewId": "idMain1",
					"viewLevel": 1
				}
			},
			"contentDensities": {
				"compact": true,
				"cozy": false
			}
		}
	}
}
