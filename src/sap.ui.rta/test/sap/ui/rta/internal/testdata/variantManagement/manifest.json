{
	"_version": "1.12.0",

	"sap.app": {
		"id": "sap.ui.rta.test.variantManagement",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "CA-UI5-FL-RTA"
	},

	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": false
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.rta.test.variantManagement.view.Main",
			"type": "XML",
			"async": true,
			"id": "app",
			"cache": {
				"keys": [
					"sap.ui.rta.test.variantManagement"
				]
			}
		},
		"handleValidation": true,
		"dependencies": {
			"minUI5Version": "${project.version}",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.dt": {},
				"sap.ui.rta": {},
				"sap.ui.layout": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.rta.test.variantManagement.i18n.i18n"
				}
			}
		}
	}
}
