		"content": {
			"row": {
				"columns": [
					{
						"title": "Sales Order",
						"value": "{salesOrder}",
						"identifier": true
					},
					{
						"title": "Customer",
						"value": "{customerName}"
					},
					{
						"title": "Net Amount",
						"value": "{netAmount}",
						"hAlign": "End"
					},
					{
						"title": "Status",
						"value": "{status}",
						"state": "{statusState}"
					},
					{
						"title": "Delivery Progress",
						"progressIndicator": {
							"percent": "{deliveryProgress}",
							"text": "{= format.percent(${deliveryProgress} / 100)}",
							"state": "{statusState}"
						}
					}
				]
			},
			"group": {
				"title": "{= ${deliveryProgress} > 10 ? 'In Delivery' : 'Not in Delivery'}",
				"order": {
					"path": "statusState",
					"dir": "ASC"
				}
