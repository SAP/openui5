				},
				"row": {
					"columns": [
						{
							"title": "Order ID",
							"width": "25%",
							"value": "{orderId}",
							"url": "{orderUrl}"
						},
						{
							"title": "Avatar",
							"width": "15%",
							"icon": {
								"src": "{icon}",
								"shape": "Square"
							}
						},
						{
							"title": "Created By",
							"value": "{name}"
						},
						{
							"title": "Status",
							"width": "20%",
							"value": "{status}",
							"state": "{statusState}"
						}
					]
				}
			}
		}
	},
