{
	"tablecard1": {
		"sap.app": {
			"id": "tablecard1",
			"type": "card"
		},
		"sap.card": {
			"type": "Table",
			"header": {
				"title": "Table Card",
				"subTitle": "Table Card subtitle"
			},
			"content": {
				"data": {
					"request": {
						"url": "./cardcontent/tableitems.json"
					},
					"path": "/names"
				},
				"row": {
					"columns": [
						{
							"title": "Avatar",
							"width": "12%",
							"icon": {
								"src": "{icon}"
							}
						},
						{
							"title": "First Name",
							"value": "{firstName}"
						},
						{
							"title": "Last Name",
							"value": "{lastName}"
						},
						{
							"title": "Link",
							"value": "{urlText}",
							"url": "{url}"
						},
						{
							"title": "Progress",
							"progressIndicator": {
								"percent": "{percent}",
								"text": "{percentValue}",
								"state": "{progressState}"
							}
						}
					]
				}
			}
		}
	},
	"tablecard2": {
		"sap.app": {
			"id": "tablecard2",
