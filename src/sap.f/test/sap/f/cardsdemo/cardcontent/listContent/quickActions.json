{
	"sap.app": {
		"id": "cardsdemo.listContent.quickActions2",
		"type": "card"
	},
	"sap.card": {
		"type": "List",
		"header": {
			"title": "Department Budget State",
			"subTitle": "2019",
			"icon": {
				"src": "sap-icon://collections-insight"
			}
		},
		"content": {
			"data": {
				"request": {
					"url": "./tasks.json"
				},
				"path": "/"
			},
			"maxItems": 3,
			"item": {
				"title": {
					"value": "{Name}"
				},
				"description": {
					"value": "Description"
				},
				"info": {
					"value": "{= ${departmentBudget}  > ${departmentBudgetThreshold} ? 'Exceeded' : 'In Range' }",
					"state": "{= ${departmentBudget}  > ${departmentBudgetThreshold} ? 'Error' : 'Success' }"
				},
				"actions": [
					{
						"type": "Navigation"
					}
				],
				"actionsStrip": [
					{
						"type": "Link",
						"text": "Link to Meal",
						"icon": "sap-icon://action",
						"actions": [
							{
								"type": "Navigation",
								"parameters": {
									"url": "http://sap.com"
								}
							}
						]
					},
					{
						"type": "ToolbarSpacer"
					},
					{
						"buttonType": "Reject",
						"overflowPriority": "High",
						"overflowGroup": 1,
						"text": "Delete",
						"actions": [
							{
								"type": "Custom",
								"parameters": {
									"item": "{}"
								}
							}
						]
					},
					{
						"buttonType": "Transparent",
						"overflowPriority": "High",
						"text": "Reject",
						"actions": [
							{
								"type": "Navigation",
								"parameters": {
									"url": "http://sap.com"
								}
							}
						]
					},
					{
						"icon": "sap-icon://email",
						"text": "Contact",
						"tooltip": "Contact tooltip",
						"actions": [
							{
								"type": "Navigation",
								"parameters": {
									"url": "http://sap.com"
								}
							}
						]
					},
					{
						"buttonType": "Reject",
						"icon": "sap-icon://email",
