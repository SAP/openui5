						"actions": [
							{
								"type": "Navigation",
								"parameters": {
									"url": "http://sap.com"
								}
							}
						]
					},
					{
						"buttonType": "Reject",
						"icon": "sap-icon://email",
						"text": "text only",
						"actions": [
							{
								"type": "Navigation",
								"parameters": {
									"url": "http://sap.com"
								}
							}
						]
					}
				]
			}
		},
		"footer": {
			"actionsStrip": [
				{
					"type": "Link",
					"text": "Link to Meal",
					"icon": "sap-icon://action",
					"actions": [
						{
							"type": "Navigation",
							"parameters": {
								"url": "http://sap.com"
							}
						}
					]
				},
				{
