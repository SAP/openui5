{
	"sap.app": {
		"type": "card",
		"id": "sap.f.cardsdemo.webpage.cardWithExtension",
		"title": "Sample of a WebPage Card",
		"subTitle" :"Sample of a WebPage Card with Extension",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"shortTitle": "A short title for this Card",
		"info": "Additional information about this Card",
		"description": "A long description for this Card",
		"tags": {
			"keywords": [
				"WebPage",
				"Card",
				"Sample"
			]
		}
	},
	"sap.ui": {
		"technology": "UI5"
	},
	"sap.card": {
		"type": "WebPage",
		"extension": "./webpageCardSampleExtension",
		"data": {
			"extension": {
				"method": "getData"
			}
		},
		"configuration": {
			"parameters": {
				"minHeight": {
					"description": "Minimum height of the content",
					"type": "string",
					"value": "30rem"
				}
			}
		},
		"header": {
			"title": "WebPage Card with Extension",
			"icon": {
				"src": "sap-icon://desktop-mobile"
			}
		},
		"content": {
			"src": "{/url}",
			"sandbox": "allow-scripts",
			"minHeight": "{parameters>/minHeight/value}"
		}
	}
}