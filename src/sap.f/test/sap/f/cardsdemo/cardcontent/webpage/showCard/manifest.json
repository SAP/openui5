{
	"sap.app": {
		"id": "sap.f.cardsdemo.webpage.showCard",
		"type": "card",
		"title": "Sample of a Snack Card",
		"subTitle": "Sample of a Snack Card",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"shortTitle": "A short title for this Card",
		"info": "Additional information about this Card",
		"description": "A long description for this Card",
		"tags": {
			"keywords": [
				"Object",
				"Card",
				"Sample",
				"Snack"
			]
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://switch-classes"
		}
	},
	"sap.card": {
		"type": "Object",
		"data": {
			"json": {
				"footerVisible": true,
				"firstName": "Donna",
				"lastName": "Moore",
				"position": "Sales Executive",
				"phone": "+1 202 555 5555",
				"email": "my@mymail.com",
				"photo": "./DonnaMoore.png",
				"agendaUrl": "/agenda",
				"company": {
					"name": "Robert Brown Entertainment",
					"address": "481 West Street, Anytown OH 45066, USA",
					"email": "mail@mycompany.com",
					"emailSubject": "Subject"
				}
			}
		},
		"header": {
			"icon": {
				"src": ""
			},
			"title": "Request from {firstName} {lastName}",
			"subTitle": "{position}"
		},
		"content": {
			"groups": [
				{
					"title": "Contact Details",
					"items": [
						{
							"label": "First Name",
							"value": "{firstName}"
						},
						{
							"label": "Last Name",
							"value": "{lastName}"
						},
						{
							"label": "Phone",
							"value": "{phone}",
							"actions": [
								{
									"type": "Navigation",
									"parameters": {
										"url": "tel:{phone}"
									}
								}
							]
						}
					]
				},
				{
					"title": "Company Details",
					"items": [
						{
							"label": "Company Name",
							"value": "{company/name}"
						},
						{
							"label": "Address",
							"value": "{company/address}"
						}
					]
				}
			]
		},
		"footer": {
			"actionsStrip": [
				{
					"text": "Show Website",
					"buttonType": "Accept",
					"actions": [
						{
							"type": "ShowCard",
							"parameters": {
								"manifest": "./webpageManifest.json"
							}
						}
					]
				}
			]
		}
	}
}