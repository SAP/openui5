{
	"_version": "1.14.0",
	"sap.app": {
		"id": "list.card",
		"type": "card",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://list"
		}
	},
	"sap.card": {
		"type": "List",
		"data": {
			"json": {
				"items": [
					{"title": "Item 1"},
					{"title": "Item 2"},
					{"title": "Item 3"},
					{"title": "Item 4"}
				]
			}
		},
		"header": {
			"actions": [{
				"type": "Navigation",
				"parameters": {
					"url": "https://sap.com",
					"target": "_blank"
				}
			}],
			"title": "Action on header level",
			"subTitle": "sematicRole - listitem"
		},
		"content": {
			"data": {
				"path": "/items"
			},
			"item": {
				"title": "{title}"
			},
			"maxItems": 4
		}				
	}
}