<mvc:View xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.m"
		xmlns:f="sap.f"
		xmlns:w="sap.ui.integration.widgets"
		width="100%"
		displayBlock="true">

	<f:GridContainer class="sapUiTinyMargin">
		<w:Card id="card1" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/listContent/quickActions.json">
			<w:layoutData>
				<f:GridContainerItemLayoutData minRows="4" columns="4" />
			</w:layoutData>
		</w:Card>
		<w:Card id="card2" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/tablecontent/groups.json">
			<w:layoutData>
				<f:GridContainerItemLayoutData minRows="4" columns="6" />
			</w:layoutData>
		</w:Card>
		<w:Card id="card3" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/staticData.json">
			<w:layoutData>
				<f:GridContainerItemLayoutData minRows="4" columns="4" />
			</w:layoutData>
		</w:Card>
	</f:GridContainer>

</mvc:View>
