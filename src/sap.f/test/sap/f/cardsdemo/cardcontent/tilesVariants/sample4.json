{
	"sap.app": {
		"id": "cardsdemo.tilesVariants.sample4",
		"type": "card"
	},
	"sap.card": {
		"type": "List",
		"header": {
			"data": {
				"request": {
					"url": "./data4.json"
					}
			},
			"title": "{title}",
			"subTitle": "{subTitle}",
			"icon": {
				"src": "{icon/src}",
				"shape": "{icon/shape}"
			},
			"actions": [
				{
					"type": "Navigation",
					"parameters": {
						"url": "http://sap.com"
					}
				}
			]
		},
		"content": {
			"data": {
				"json": [{
						"Name": "Comfort Easy"
					}
				]
			},
			"item": {
				"title": "{Name}"
			}
		}
	}
}
