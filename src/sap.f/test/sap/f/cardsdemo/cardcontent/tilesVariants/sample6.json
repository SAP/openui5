{
	"sap.app": {
		"id": "cardsdemo.tilesVariants.sample6",
		"type": "card"
	},
	"sap.card": {
		"type": "List",
		"header": {
			"type": "Numeric",
			"data": {
				"request": {
					"url": "./data6.json"
					}
			},
			"title": "{title}",
			"subTitle": "{subTitle}",
			"wrappingType": "Hyphenated",
			"unitOfMeasurement": "{unitOfMeasurement}",
			"details": {
				"text": "{details/text}",
				"state": "Success"
			},
			"detailsMaxLines": 2,
			"actions": [
				{
					"type": "Navigation",
					"parameters": {
						"url": "http://sap.com"
					}
				}
			]
		},
		"content": {
			"data": {
				"json": [{
						"Name": "Comfort Easy"
					}
				]
			},
			"item": {
				"title": "{Name}"
			}
		}
	}
}