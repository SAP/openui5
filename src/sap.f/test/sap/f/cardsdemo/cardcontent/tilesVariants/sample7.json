{
	"sap.app": {
		"id": "cardsdemo.tilesVariants.sample7",
		"type": "card"
	},
	"sap.card": {
		"type": "List",
		"header": {
			"data": {
				"request": {
					"url": "./data7.json"
					}
			},
			"title": "{title}",
			"subtitle": "{subtitle}",
			"wrappingType": "Hyphenated",
			"actions": [
				{
					"type": "Navigation",
					"parameters": {
						"url": "http://sap.com"
					}
				}
			]
		},
		"content": {
			"data": {
				"json": [{
						"Name": "Comfort Easy"
					}
				]
			},
			"item": {
				"title": "{Name}"
			}
		}
	}
}