{
	"sap.app": {
		"id": "sap.f.cardsdemo.showCard.paginationAndActions",
		"type": "card"
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://action"
		}
	},
	"sap.card": {
		"extension": "./ActionsExtension",
		"type": "List",
		"header": {
			"title": "Pagination, host and extension actions",
			"icon": {
				"src": "sap-icon://activities"
			}
		},
		"content": {
			"data": {
				"json": [
					{ "training": "Scrum", "location": "Online" },
					{ "training": "Quality Management", "location": "Classroom Attendance" },
					{ "training": "Test Driven Development", "location": "Classroom Attendance" },
					{ "training": "Integration Cards Training", "location": "Online" }
				]
			},
			"item": {
				"title": "{training}",
				"description": "{location}"
			}
		},
		"footer": {
			"paginator": {
				"pageSize": 2
			}
		}
	}
}