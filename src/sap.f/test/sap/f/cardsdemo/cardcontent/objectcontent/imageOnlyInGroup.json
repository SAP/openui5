{
	"sap.app": {
		"id": "cardsdemo.object.imageOnlyInGroup",
		"type": "card"
	},
	"sap.card": {
		"type": "Object",
		"header": {
			"title": "Object Card",
			"subTitle": "With Full Width Image"
		},
		"content": {
			"groups": [
				{
					"alignment": "Stretch",
