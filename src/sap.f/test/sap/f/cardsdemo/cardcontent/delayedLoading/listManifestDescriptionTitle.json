{
  "sap.app": {
    "id": "cardsdemo.samples.list4",
    "type": "card"
  },
  "sap.card": {
    "type": "List",
    "header": {
      "data": {
        "request": {
          "url": "./cardsdemo/cardcontent/delayedLoading/listData2.json"
        },
        "path": "/header"
      },
      "title": "{title}",
      "subTitle": "Subtitle",
      "status": {
        "text": "2 of {count}"
      },
      "icon": {
        "src": "{src}"
      }
    },
    "content": {
      "maxItems": 8,
      "data": {
        "request": {
          "url": "./cardsdemo/cardcontent/someitems.json"
        }
      },
      "item": {
        "title": {
          "label": "Title",
          "value": "{Name}"
        },
        "description": {
          "value": "{Name}"
        }
      }
    }
  }
}