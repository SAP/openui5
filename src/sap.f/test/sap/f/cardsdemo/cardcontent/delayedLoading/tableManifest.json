{
	"sap.app": {
		"type": "card"
	},
	"sap.card": {
		"type": "Table",
		"header": {
			"type": "Numeric",
			"data": {
				"request": {
					"url": "./cardcontent/delayedLoading/tableData.json"
				},
				"path": "/header"
			},
			"title": "{title}",
			"mainIndicator": {
				"number": "{mainIndicator/number}"
			}
		},
		"content": {
			"data": {
				"request": {
					"url": "./cardcontent/delayedLoading/tableData.json"
				},
				"path": "/content"
			},
			"numberOfItems": 9,
			"row": {
				"columns": [
					{
						"label": "Sales Order",
						"value": "{salesOrder}",
						"identifier": true
					},
					{
						"label": "Customer",
						"value": "{name}"
					},
					{
						"label": "Net Amount",
						"value": "{netAmount}"
					},
					{
						"label": "Status",
						"value": "{status}",
						"state": "{statusState}"
					}
				]
			}
		}
	}
}