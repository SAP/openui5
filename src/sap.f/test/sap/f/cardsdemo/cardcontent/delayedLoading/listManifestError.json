{
  "sap.app": {
    "id": "cardsdemo.samples.list5",
    "type": "card"
  },
  "sap.card": {
    "type": "List",
    "header": {
      "data": {
        "request": {
          "url": "./cardsdemo/cardcontent/delayedLoading/listData2.json"
        },
        "path": "/header"
      },
