<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:w="sap.ui.integration.widgets"
	displayBlock="true">
	<Page title="{i18n>appTitle}">
		<subHeader>
			<Toolbar>
				<ToolbarSpacer />
				<Title text="Default Header" />
				<ToolbarSpacer />
			</Toolbar>
		</subHeader>

		<Panel>
			<w:Card
				id="default"
				manifest="{manifests>/defaultHeader/card1}"
				width="300px"
				class="sapUiSmallMargin" />
			<w:Card
				id="default1x1"
				manifest="{manifests>/defaultHeader/card2}"
				width="300px"
				class="sapUiSmallMargin" />
			<w:Card
				id="default2x1"
				manifest="{manifests>/defaultHeader/card3}"
				width="300px"
				class="sapUiSmallMargin" />
			<w:Card
				id="stackedColumn"
				manifest="{manifests>/defaultHeader/stackedColumn}"
				height="500px"
				width="700px"
				class="sapUiSmallMargin" />
			<w:Card
				id="default4x1"
				manifest="{manifests>/defaultHeader/card4}"
				width="300px"
				class="sapUiSmallMargin" />
			<w:Card
				id="default2x2"
				manifest="{manifests>/defaultHeader/card5}"
				width="300px"
				height="200px"
				class="sapUiSmallMargin" />
			<w:Card
				id="line"
				manifest="{manifests>/defaultHeader/line}"
				height="500px"
				width="700px"
				class="sapUiSmallMargin" />
			<w:Card
				id="default4x2"
				manifest="{manifests>/defaultHeader/card6}"
				width="320px"
				class="sapUiSmallMargin" />
			<w:Card
				id="default4x4"
				manifest="{manifests>/defaultHeader/card7}"
				width="320px"
				class="sapUiSmallMargin" />
			<w:Card
				id="stackedBar"
				manifest="{manifests>/defaultHeader/stackedBar}"
				height="500px"
				width="700px"
				class="sapUiSmallMargin" />
			<w:Card
				id="donut"
				manifest="{manifests>/defaultHeader/donut}"
				height="500px"
				width="700px"
				class="sapUiSmallMargin" />
		</Panel>
	</Page>
</mvc:View>