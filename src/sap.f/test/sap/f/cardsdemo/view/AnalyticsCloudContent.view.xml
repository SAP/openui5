<mvc:View
	controllerName="sap.f.cardsdemo.controller.AnalyticsCloudContent"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	xmlns:w="sap.ui.integration.widgets"
	displayBlock="true">
	<Page title="{i18n>appTitle}">
		<subHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="AnalyticsCloud Content Card "/>
				<ToolbarSpacer/>
			</Toolbar>
		</subHeader>
		<VBox class="sapUiSmallMargin">
			<FormattedText htmlText="To run this sample:"></FormattedText>
			<FormattedText htmlText="1. You need to run the browser with disabled CORS checks."></FormattedText>
			<FormattedText htmlText="&#160;&#160;&#160;&#160;For Windows: [chrome dir]./chrome.exe --user-data-dir=&#x22;C://chrome-dev-disabled-security&#x22; --disable-web-security"></FormattedText>
			<FormattedText htmlText="2. There will be a popup with login which is required, see related Jira item for credentials."></FormattedText>
		</VBox>

		<w:Card id="card1" width="25rem" class="sapUiSmallMargin" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/analyticsCloud/sample1.json">
		</w:Card>
		<w:Card id="card2" width="25rem" class="sapUiSmallMargin" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/analyticsCloud/sample1.json">
		</w:Card>
	</Page>
</mvc:View>