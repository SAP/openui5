<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	xmlns:w="sap.ui.integration.widgets"
	displayBlock="true">

	<Page title="{i18n>appTitle}">

		<subHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="AnalyticsCloud Content Card "/>
				<ToolbarSpacer/>
			</Toolbar>
		</subHeader>

		<content>
			<MessageStrip
				class="sapUiSmallMargin"
				type="Warning"
				showIcon="true"
				text="To run this sample check for the instructions in the related BLI."
				/>

			<f:GridContainer class="sapUiSmallMargin">
				<w:Card manifest="./cardsdemo/cardcontent/analyticsCloud/manifest1.json">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="4" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="./cardsdemo/cardcontent/analyticsCloud/manifest2.json">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="6" />
					</w:layoutData>
				</w:Card>
			</f:GridContainer>
		</content>
	</Page>
</mvc:View>