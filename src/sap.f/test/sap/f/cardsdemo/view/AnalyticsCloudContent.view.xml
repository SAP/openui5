<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	xmlns:w="sap.ui.integration.widgets"
	displayBlock="true">
	<Page title="{i18n>appTitle}">
		<subHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="AnalyticsCloud Content Card "/>
				<ToolbarSpacer/>
			</Toolbar>
		</subHeader>
		<mvc:View viewName="sap.f.cardsdemo.view.common.CommonCardSettings" type="XML" async="true"></mvc:View>

		<f:GridContainer class="sapUiSmallMargin">
			<w:Card id="card1" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/analyticsCloud/manifestLine1.json">
				<w:layoutData>
					<f:GridContainerItemLayoutData columns="4" />
				</w:layoutData>
			</w:Card>
			<w:Card id="card2" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/analyticsCloud/manifestLine2.json">
				<w:layoutData>
					<f:GridContainerItemLayoutData columns="6" />
				</w:layoutData>
			</w:Card>
			<w:Card id="card3" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/analyticsCloud/manifestArea.json">
				<w:layoutData>
					<f:GridContainerItemLayoutData columns="6" />
				</w:layoutData>
			</w:Card>
			<w:Card id="card4" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/analyticsCloud/manifestColumn.json">
				<w:layoutData>
					<f:GridContainerItemLayoutData columns="6" />
				</w:layoutData>
			</w:Card>
			<w:Card  id="card5" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/analyticsCloud/manifestBar.json">
				<w:layoutData>
					<f:GridContainerItemLayoutData columns="6" />
				</w:layoutData>
			</w:Card>
		</f:GridContainer>
	</Page>
</mvc:View>