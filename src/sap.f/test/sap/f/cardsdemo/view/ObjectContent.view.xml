<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:w="sap.ui.integration.widgets"
	displayBlock="true"
	controllerName="sap.f.cardsdemo.controller.ObjectContent">
	<Page title="{i18n>appTitle}">
		<subHeader>
			<Toolbar>
				<ToolbarSpacer />
				<Title text="Object Content Card" />
				<ToolbarSpacer />
			</Toolbar>
		</subHeader>
		<mvc:View viewName="sap.f.cardsdemo.view.common.CommonCardSettings" type="XML" async="true"></mvc:View>
		<Panel>
			<w:Card
				id="cardDataHandling"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/datahandling_manifest.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardLongLabelAvatar"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/manifest.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/manifest.json"
				width="400px"
				class="sapUiTinyMargin" />
			<w:Card
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/manifest.json"
				width="400px"
				height="275px"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardStaticData"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/staticData.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardSnackAndObjectStatus"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/object_snack.manifest.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/object_bite.manifest.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/withExtension/manifest.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardForm"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/form.json"
				width="400px"
				class="sapUiTinyMargin" />
			<w:Card id="card4" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/titleMaxLines_labelWrapping.json"
				width="400px"
				class="sapUiTinyMargin" />
			<w:Card
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/formWithValidations.json"
				width="400px"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImageNoHeader"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/imageNoHeader.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImage"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/image.json"
				width="400px"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImageOnlyInGroup"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/imageOnlyInGroup.json"
				width="400px"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImageSmall"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/imageSmall.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImageOnly"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/imageOnly.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardJustImage"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/justImage.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImageOnlyWithFooter"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/imageOnlyWithFooter.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImgOvWithFooter2"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/imageAndOverlayWithFooter2.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImageWithFooter"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/imageWithFooter.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImgOvWithFooter"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/imageAndOverlayWithFooter.json"
				width="auto"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImageNoHeaderNoFooter"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/imageNoHeaderAndNoFooter.json"
				width="400px"
				class="sapUiTinyMargin" />
				<w:Card
			id="cardImgOvNoHeaderNoFooter"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/imageAndOverlayNoHeaderAndNoFooter.json"
				width="400px"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImagePositionNoHeaderNoFooter"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/imagePositionNoHeaderNoFooter.json"
				width="600px"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardJustImageWithOverlay"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/justImageWithTextOverlay.json"
				width="auto"
				class="sapUiTinyMargin" />
			<VBox width="500px">
				<w:Card
					id="cardImgOvTextPosition"
					manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/justImageWithOverlayTextPosition.json"
					width="auto"
					class="sapUiTinyMargin" />
			</VBox>
			<w:Card
				id="cardImgOverlayGradient"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/justImageWithOverlayGradient.json"
				width="600px"
				height="200px"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImgOvImagePosition"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/justImageWithOverlayImagePosition.json"
				width="600px"
				height="200px"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImgOvNotFullWidth"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/justImageWithOverlayNotFullWidth.json"
				width="600px"
				height="200px"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImgOvCalcHeight"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/justImageWithOverlayCalcHeight.json"
				width="600px"
				class="sapUiTinyMargin" />
			<w:Card
				id="cardImgOvImgHeight"
				manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/objectcontent/justImageWithOverlayImgHeight.json"
				width="600px"
				class="sapUiTinyMargin" />
		</Panel>
	</Page>
</mvc:View>