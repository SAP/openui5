<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	xmlns:w="sap.ui.integration.widgets"
	controllerName="sap.f.cardsdemo.controller.NavigationService"
	displayBlock="true">
	<Page title="{i18n>appTitle}">
		<subHeader>
			<Toolbar>
				<ToolbarSpacer />
				<Title text="Navigation scenarios" />
				<ToolbarSpacer />
			</Toolbar>
		</subHeader>

		<Panel>
			<VBox>
				<VBox class="sapUiMediumMarginLeft">
					<Text text="Cards using navigation service. - List and Table Content" />
				</VBox>
				<HBox>
					<w:Card
						action=".onActionLog"
						manifest="./cardsdemo/cardcontent/navigationService/manifest_navigationService.json"
						width="400px"
						class="sapUiMediumMargin" />
					<w:Card
						action=".onActionLog"
						manifest="./cardsdemo/cardcontent/navigationService/manifest_navigationService2.json"
						width="400px"
						class="sapUiMediumMargin" />
					<w:Card
						action=".onActionLog"
						manifest="./cardsdemo/cardcontent/navigationService/manifest_navigationService3.json"
						width="400px"
						class="sapUiMediumMargin" />
					<w:Card
						action=".onActionLog"
						manifest="./cardsdemo/cardcontent/navigationService/manifest_navigationService4.json"
						width="400px"
						class="sapUiMediumMargin" />
				</HBox>
				<VBox class="sapUiMediumMarginLeft">
					<Text text="Cards using navigation service with hidden items. - List, Table and TimeLine Content" />
				</VBox>
				<HBox>
					<w:Card
						width="400px"
						action=".onActionLog"
						manifest="./cardsdemo/cardcontent/navigationService/hiddenItems/manifest_navigation_hidden_list.json"
						class="sapUiMediumMargin" />
					<w:Card
						width="400px"
						action=".onActionLog"
						manifest="./cardsdemo/cardcontent/navigationService/hiddenItems/manifest_navigation_hidden_table.json"
						class="sapUiMediumMargin" />
					<w:Card
						width="400px"
						action=".onActionLog"
						manifest="./cardsdemo/cardcontent/navigationService/hiddenItems/manifest_navigation_hidden_timeline.json"
						class="sapUiMediumMargin" />
				</HBox>
				<VBox class="sapUiMediumMarginLeft">
					<Text text="Cards using navigation service. - Analytical Content" />
				</VBox>
				<HBox>
					<w:Card
						class="sapUiMediumMargin"
						manifest="./cardsdemo/cardcontent/navigationService/manifest_navigationAction_url_analyticalContent.json"
						height="500px"
						width="500px" />
					<w:Card
						class="sapUiMediumMargin"
						manifest="./cardsdemo/cardcontent/navigationService/manifest_navigationAction_service_analyticalContent.json"
						height="500px"
						width="500px" />
				</HBox>
				<VBox class="sapUiMediumMarginLeft">
					<Text text="Cards using navigation service. - Object Content" />
				</VBox>
				<HBox>
					<w:Card
						manifest="./cardsdemo/cardcontent/navigationService/manifest_navigation_objectContent.json"
						height="500px"
						width="500px"
						class="sapUiMediumMargin" />
					<w:Card
						class="sapUiMediumMargin"
						manifest="./cardsdemo/cardcontent/navigationService/manifest_navigation_object_service.json"
						width="auto"
						height="500px" />
				</HBox>
				<VBox class="sapUiMediumMarginLeft">
					<Text text="Cards using navigation action event." />
				</VBox>
				<HBox>
					<w:Card
						action=".onAction"
						width="400px"
						manifest="./cardsdemo/cardcontent/navigationService/manifest_navigationAction.json"
						class="sapUiMediumMargin" />
					<w:Card
						action=".onAction"
						width="400px"
						manifest="./cardsdemo/cardcontent/navigationService/manifest_navigationActionTable.json"
						class="sapUiMediumMargin" />
				</HBox>
				<VBox class="sapUiMediumMarginLeft">
					<Text text="Cards using navigation service. - Timeline Content" />
				</VBox>
				<HBox>
					<w:Card
						id="timeline"
						class="sapUiMediumMargin"
						manifest="{manifests>/timelineContent/card0}"
						width="300px" />
				</HBox>
			</VBox>
		</Panel>
	</Page>
</mvc:View>