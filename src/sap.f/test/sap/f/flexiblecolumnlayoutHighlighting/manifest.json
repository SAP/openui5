{
	"_version": "2.0.1",
	"sap.app": {
		"id": "flexiblecolumnlayoutHighlighting",
		"type": "application",
		"title": "Application 'flexiblecolumnlayoutHighlighting'",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.98.0",
			"libs": {
				"sap.f": {},
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.uxap": {}
			}
		},
		"config": {
			"fullWidth": true
		},
		"rootView": "flexiblecolumnlayoutHighlighting.FlexibleColumnLayout",
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "flexiblecolumnlayoutHighlighting",
				"controlId": "fcl",
				"transition": "slide",
				"bypassed": {}
			},
			"routes": [
				{
					"pattern": "page2",
					"name": "page2",
					"target": "page2",
					"layout": "EndColumnFullScreen"
				},
				{
					"pattern": "page3",
					"name": "page3",
					"target": "page3",
					"layout": "EndColumnFullScreen"
				},
				{
					"pattern": ":layout:",
					"name": "list",
					"target": [
						"list",
						"detailEmpty"
					]
				},
				{
					"pattern": "detail/{layout}",
					"name": "detail",
					"target": [
						"list",
						"detail"
					]
				},
				{
					"pattern": "detailDetail/{layout}",
					"name": "detailDetail",
					"target": [
						"list",
						"detail",
						"detailDetail"
					]
				}
			],
			"targets": {
				"list": {
					"name": "List",
					"controlAggregation": "beginColumnPages"
				},
				"detail": {
					"name": "Detail",
					"controlAggregation": "midColumnPages"
				},
				"detailDetail": {
					"name": "DetailDetail",
					"controlAggregation": "endColumnPages"
				},
				"page2": {
					"name": "Page2",
					"controlAggregation": "endColumnPages"
				},
				"page3": {
					"name": "Page3",
					"controlAggregation": "endColumnPages"
				},
				"detailEmpty": {
					"name": "DetailEmpty",
					"controlAggregation": "midColumnPages"
				}
			}
		}
	}
}