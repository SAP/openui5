<mvc:View
	xmlns="sap.f"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	controllerName="flexiblecolumnlayout.FlexibleColumnLayout"
	height="100%">
	<FlexibleColumnLayout
		id="fcl"
		stateChange="onStateChanged"
		layout="{/layout}"
		backgroundDesign="Solid"
		restoreFocusOnBackNavigation="true" />
</mvc:View>
