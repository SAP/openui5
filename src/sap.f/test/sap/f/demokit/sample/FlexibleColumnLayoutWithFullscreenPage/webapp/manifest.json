{
	"_version": "2.0.2",
	"sap.app": {
		"id": "sap.f.sample.FlexibleColumnLayoutWithFullscreenPage",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"type": "application",
		"title": "Application 'FlexibleColumnLayoutWithFullscreenPage'"
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.f.FlexibleColumnLayoutWithFullscreenPage.view.FlexibleColumnLayout",
			"id": "fcl"
		},
		"dependencies": {
			"minUI5Version": "1.98.0",
			"libs": {
				"sap.f": {},
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.uxap": {}
			}
		},
		"config": {
			"fullWidth": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "sap.f.FlexibleColumnLayoutWithFullscreenPage.view",
				"controlId": "fcl",
				"transition": "slide",
				"bypassed": {}
			},
			"routes": [
				{
					"pattern": ":layout:",
					"name": "list",
					"target": [
						"list"
					]
				},
				{
					"pattern": "detail/{category}/{layout}",
					"name": "detail",
					"target": [
						"detail"
					]
				},
				{
					"pattern": "detailDetail/{category}/{product}/{layout}",
					"name": "detailDetail",
					"target": [
						"detail",
						"detailDetail"
					]
				},
				{
					"pattern": "detailDetailDetail/{category}/{product}/{supplier}/{layout}",
					"name": "detailDetailDetail",
					"target": [
						"detail",
						"detailDetail",
						"detailDetailDetail"
					]
				}
			],
			"targets": {
				"list": {
					"name": "List",
					"controlAggregation": "beginColumnPages"
				},
				"detail": {
					"name": "Detail",
					"controlAggregation": "beginColumnPages"
				},
				"detailDetail": {
					"name": "DetailDetail",
					"controlAggregation": "midColumnPages"
				},
				"detailDetailDetail": {
					"name": "DetailDetailDetail",
					"controlAggregation": "endColumnPages"
				}
			}
		}
	}
}