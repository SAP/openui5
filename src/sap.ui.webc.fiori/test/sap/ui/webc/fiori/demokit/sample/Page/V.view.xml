<mvc:View xmlns="sap.ui.webc.main" xmlns:fiori="sap.ui.webc.fiori" xmlns:mvc="sap.ui.core.mvc" height="100%">
	<fiori:Page height="100%" width="100%">
		<fiori:header>
			<fiori:Bar>
				<fiori:startContent>
					<Button icon="nav-back" accessibleName="Back" design="Transparent"></Button>
				</fiori:startContent>
				<fiori:middleContent>
					<Title text="Page Title"></Title>
				</fiori:middleContent>
				<fiori:endContent>
					<Button icon="action" accessibleName="Share"></Button>
				</fiori:endContent>
			</fiori:Bar>
		</fiori:header>
		<fiori:content>
			<fiori:IllustratedMessage>
				<fiori:actions>
					<Button icon="refresh" text="Try Again"></Button>
				</fiori:actions>
			</fiori:IllustratedMessage>
		</fiori:content>
		<fiori:footer>
			<fiori:Bar design="Footer">
				<fiori:endContent>
					<Button text="Accept" design="Positive" />
					<Button text="Reject" design="Negative" />
					<Button text="Edit" />
					<Button text="Delete" />
				</fiori:endContent>
			</fiori:Bar>
		</fiori:footer>
	</fiori:Page>
</mvc:View>