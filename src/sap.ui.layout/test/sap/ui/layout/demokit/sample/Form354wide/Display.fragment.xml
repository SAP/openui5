<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:Form id="FormDisplay354wide"
			editable="false">
			<f:title>
				<core:Title text="Address" />
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout
					labelSpanXL="4"
					labelSpanL="3"
					labelSpanM="4"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="0"
					emptySpanL="4"
					emptySpanM="0"
					emptySpanS="0"
					columnsXL="2"
					columnsL="1"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer>
					<f:formElements>
						<f:FormElement label="Full name of the contact">
							<f:fields>
								<Text text="{SupplierName}" id="nameText"  />
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Street and number where the contact is located">
							<f:fields>
								<Text text="{Street} {HouseNumber}" />
							</f:fields>
						</f:FormElement>
						<f:FormElement label="ZIP Code and City">
							<f:fields>
								<Text text="{ZIPCode} {City}" />
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Country where the contact is located">
							<f:fields>
								<Text text="{Country}" id="countryText" />
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>