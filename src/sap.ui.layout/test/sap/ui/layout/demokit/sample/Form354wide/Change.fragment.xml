<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:Form id="FormChange354wide"
			editable="true">
			<f:title>
				<core:Title text="Address" />
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout
					labelSpanXL="4"
					labelSpanL="3"
					labelSpanM="4"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="0"
					emptySpanL="4"
					emptySpanM="0"
					emptySpanS="0"
					columnsXL="2"
					columnsL="1"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer>
					<f:formElements>
						<f:FormElement label="Full name of the contact">
							<f:fields>
								<Input value="{SupplierName}" id="name"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Street and number where the contact is located">
							<f:fields>
								<Input value="{Street}" />
								<Input value="{HouseNumber}">
									<layoutData>
										<l:GridData span="XL1 L2 M2 S4" />
									</layoutData>
								</Input>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="ZIP Code and City">
							<f:fields>
								<Input value="{ZIPCode}">
									<layoutData>
										<l:GridData span="XL1 L2 M2 S4" />
									</layoutData>
								</Input>
								<Input value="{City}" />
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Country where the contact is located">
							<f:fields>
								<Select id="country" selectedKey="{Country}">
									<items>
										<core:Item text="England" key="England"/>
										<core:Item text="Germany" key="Germany"/>
										<core:Item text="USA" key="USA"/>
									</items>
								</Select>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>