<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormChange480"
			editable="true"
			layout="ResponsiveGridLayout"
			title="Address"
			labelSpanXL="4"
			labelSpanL="4"
			labelSpanM="4"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="0"
			emptySpanL="0"
			emptySpanM="0"
			emptySpanS="0"
			columnsXL="2"
			columnsL="2"
			columnsM="1"
			singleContainerFullSize="false" >
			<f:content>
				<core:Title text="Office" />
				<Label text="Name" />
				<Input id="name" value="{SupplierName}" />
				<Label text="Street/No." />
				<Input value="{Street}">
				</Input>
				<Input value="{HouseNumber}">
					<layoutData>
						<l:GridData span="XL2 L3 M3 S4" />
					</layoutData>
				</Input>
				<Label text="ZIP Code/City" />
				<Input value="{ZIPCode}">
					<layoutData>
						<l:GridData span="XL2 L3 M3 S4" />
					</layoutData>
				</Input>
				<Input value="{City}" />
				<Label text="Country" />
				<Select id="country" selectedKey="{Country}">
					<items>
						<core:Item text="England" key="England"/>
						<core:Item text="Germany" key="Germany"/>
						<core:Item text="USA" key="USA"/>
					</items>
				</Select>
				<core:Title text="Online" />
				<Label text="Web" />
				<Input value="{Url}" />
				<Label text="Twitter" />
				<Input value="{Twitter}" />
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>