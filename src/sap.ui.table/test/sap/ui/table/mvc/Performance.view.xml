<mvc:View xmlns="sap.m"
		  xmlns:core="sap.ui.core"
		  xmlns:mvc="sap.ui.core.mvc"
		  controllerName="sap.ui.table.mvc.Performance"
		  height="100%">
	<Toolbar id="" enbabled="false">
		<Button text="Settings" press="onSettingsPress" type="Emphasized"/>
		<ToolbarSpacer/>
		<Button text="Create" press="onCreateTablePress"/>
		<Button text="Render" press="onRenderTablePress"/>
		<Button text="Bind" press="onBindTablePress"/>
		<Button text="Unbind" press="onUnbindTablePress"/>
		<Button text="Destroy" press="onDestroyTablePress"/>
		<ToolbarSeparator/>
		<Button text="Run" press="onRunPress" type="Accept"/>
	</Toolbar>
	<core:HTML content="&lt;div id=&quot;tableContainer&quot;&gt;&lt;/div&gt;" preferDOM="false"/>
	<core:HTML content="&lt;div id=&quot;results&quot;&gt;&lt;/div&gt;" preferDOM="false"/>
</mvc:View>