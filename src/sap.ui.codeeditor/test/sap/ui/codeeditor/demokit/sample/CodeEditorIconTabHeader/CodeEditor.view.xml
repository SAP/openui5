<mvc:View
	controllerName="sap.ui.codeeditor.sample.CodeEditorIconTabHeader.CodeEditor"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:ce="sap.ui.codeeditor"
	height="100%">
	<IconTabHeader
		id="iconTabHeader"
		selectedKey="invalidKey"
		select=".onSelectTab">
		<items>
			<IconTabFilter text="A" key="A" />
			<IconTabFilter text="B" key="B" />
		</items>
	</IconTabHeader>
	<ce:CodeEditor id="aCodeEditor" height="300px" type="javascript" />
</mvc:View>