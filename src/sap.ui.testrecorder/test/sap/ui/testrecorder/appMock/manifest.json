{
	"_version": "1.12.0",

	"sap.app": {
		"id": "appMock",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "appMock.view.Main",
			"type": "XML",
			"id": "main"
		},
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "appMock.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [{
				"pattern": "",
				"name": "home",
				"target": "home"
			}],
			"targets": {
				"home": {
					"viewId": "main",
					"viewName": "Main",
					"viewLevel" : 1
				}
			}
		}
	}
}