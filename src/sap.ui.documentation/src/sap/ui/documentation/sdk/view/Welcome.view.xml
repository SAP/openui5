<mvc:View
		controllerName="sap.ui.documentation.sdk.controller.Welcome"
		xmlns="sap.m"
		xmlns:l="sap.ui.layout"
		xmlns:core="sap.ui.core"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:custom="sap.ui.documentation.sdk.controls">

	<Page showHeader="false">
		<l:BlockLayout>
			<l:BlockLayoutRow>
				<l:BlockLayoutCell class="sapUiNoContentPadding sapUiDemokitLandingImageCell">
					<Image
							src="./resources/sap/ui/documentation/sdk/images/Home_S_cropped_280279.jpg"
							width="100%"
							densityAware="false"
							visible="{device>/system/phone}"/>
					<Image
							src="./resources/sap/ui/documentation/sdk/images/Home_M_and_L_cropped.jpg"
							width="100%"
							densityAware="false"
							visible="{=!${device>/system/phone}}"/>
					<Text class="sapUiDocumentationWelcomeHeaderText sapUiDocumentationWelcomeHeaderTitleText"
						  text="You have arrived!" width="100%" textAlign="Center"/>
					<Text class="sapUiDocumentationWelcomeHeaderText sapUiDocumentationWelcomeHeaderSubText"
						  text="Welcome to the new Demo Kit" width="100%" textAlign="Center"/>
				</l:BlockLayoutCell>
			</l:BlockLayoutRow>

			<l:BlockLayoutRow>
				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternA sapUiBlockCenteredCell sapUiDemokitLandingBlockHeadline">
					<custom:ActionButton
							text="Get Started with UI5"
							type="Emphasized"
							press="onGetStarted"
							class="sapUiDocumentationWelcomeHeaderButton"/>
				</l:BlockLayoutCell>
			</l:BlockLayoutRow>

			<l:BlockLayoutRow>
				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternE">
					<l:VerticalLayout
							width="100%">
						<core:Icon
								src="sap-icon://course-book"
								size="2.5rem"
								color="Default"/>
						<custom:TitleLink
								href="#/topic"
								text="Documentation"
								titleStyle="H2"
								class="sapUiTinyMarginTopBottom"/>
						<Text
								text="UI5 offers a solid foundation for building enterprise-ready apps. Read more about its powerful development paradigms."/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternD">
					<l:VerticalLayout
							width="100%">
						<core:Icon
								src="sap-icon://explorer"
								size="2.5rem"
								color="Default"/>
						<custom:TitleLink
								href="#/controls"
								text="Samples"
								titleStyle="H2"
								class="sapUiTinyMarginTopBottom"/>
						<Text
								text="More than 400 UI controls are waiting to be discovered. Use them out of the box, or extend and tweak them to meet your requirements."/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternC">
					<l:VerticalLayout
							width="100%">
						<core:Icon
								src="sap-icon://developer-settings"
								size="2.5rem"
								color="Default"/>
						<custom:TitleLink
								href="#/api"
								text="API Reference"
								titleStyle="H2"
								class="sapUiTinyMarginTopBottom"/>
						<Text
								text="Curious about our UI controls? Take a look at the technical details to see how flexible the controls are."/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternB">
					<l:VerticalLayout
							width="100%">
						<core:Icon
								src="sap-icon://hello-world"
								size="2.5rem"
								color="Default"/>
						<custom:TitleLink
								href="#/demoapps"
								text="Demos Apps"
								titleStyle="H2"
								class="sapUiTinyMarginTopBottom"/>
						<Text
								text="Use real-life demos or app templates as a starting point for your developments, or learn with our tutorials."/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
			</l:BlockLayoutRow>

			<l:BlockLayoutRow>
				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternA sapUiDemokitLandingBlockHeadline"
						title="Key Features"
						titleAlignment="Center"/>
			</l:BlockLayoutRow>

			<l:BlockLayoutRow>
				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternBContrast">
					<l:Grid
							defaultSpan="L6 M12 S12"
							hSpacing="0"
							vSpacing="0">
						<l:VerticalLayout
								width="100%">
							<custom:TitleLink
									href="#/topic/50eadaac8d2e49ee8996dc2b560cb76b"
									text="Responsive Across Browsers and Devices"
									titleStyle="H2"
									class="demokitInitialPagesWhiteText sapUiTinyMarginTopBottom"/>
							<Text
									text="No need to worry about device specifics! UI5 apps run on smartphones, tablets, and desktops. The UI controls automatically adapt themselves to each device's capabilities and make the most of the available real estate."
									class="demokitInitialPagesWhiteText"/>
						</l:VerticalLayout>
						<Image
								src="./resources/sap/ui/documentation/sdk/images/demoAppsTeaser.png"
								width="100%">
							<layoutData>
								<l:GridData span="L6 M12 S12"/>
							</layoutData>
						</Image>
					</l:Grid>
				</l:BlockLayoutCell>

				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternE">
					<l:VerticalLayout
							width="100%">
						<custom:TitleLink
								text="Powerful Development Concepts"
								href="#/topic/ec699e0817fb46a0817b0fa276a249f8"
								titleStyle="H2"/>
						<Text
								text="The UI5 core offers a solid foundation that simplifies your work by managing many aspects of modern development behind the scenes. It comes with built-in support for architectural concepts like MVC, two-way data binding, and routing."
								class="sapUiSmallMarginBottom"/>
						<l:HorizontalLayout>
							<core:Icon
									src="sap-icon://accept"
									size="0.875rem"
									color="Default"
									class="sapUiTinyMarginBegin sapUiSmallMarginEnd"/>
							<Text text="Includes standards like MVC and various data-binding types"/>
						</l:HorizontalLayout>
						<l:HorizontalLayout>
							<core:Icon
									src="sap-icon://accept"
									size="0.875rem"
									color="Default"
									class="sapUiTinyMarginBegin sapUiSmallMarginEnd"/>
							<Text text="Choose between different view formats (XML, HTML, JavaScript, JSON)"/>
						</l:HorizontalLayout>
						<l:HorizontalLayout>
							<core:Icon
									src="sap-icon://accept"
									size="0.875rem"
									color="Default"
									class="sapUiTinyMarginBegin sapUiSmallMarginEnd"/>
							<Text text="Binding with OData, JSON, XML and other data formats"/>
						</l:HorizontalLayout>
						<l:HorizontalLayout>
							<core:Icon
									src="sap-icon://accept"
									size="0.875rem"
									color="Default"
									class="sapUiTinyMarginBegin sapUiSmallMarginEnd"/>
							<Text text="Built-in support tool for exploring the object tree and binding status"/>
						</l:HorizontalLayout>
					</l:VerticalLayout>
				</l:BlockLayoutCell>

			</l:BlockLayoutRow>

			<l:BlockLayoutRow>
				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternC">
					<l:VerticalLayout
							width="100%">
						<custom:TitleLink
								href="#/topic/a264a9abf98d4caabbf9b027bc1005d8"
								text="Enterprise-Ready Web Toolkit"
								titleStyle="H2"/>
						<Text
								text="Created by professionals, for modern developers building state of the art web applications. It comes with all features needed to cover most current application requirements, with standards high enough to be delivered in standard SAP solutions."/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>


				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternB">
					<l:VerticalLayout
							width="100%">
						<custom:TitleLink
								href="https://experience.sap.com/fiori-design/"
								text="Award-Winning SAP Fiori Design"
								titleStyle="H2"/>
						<Text
								text="UI5 applications benefit from a consistent design language and predefined UX patterns across all frontend features. Use a predefined theme or try our online theming tool to create a custom theme."
								class="sapUiSmallMarginBottom"/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>

			</l:BlockLayoutRow>

			<l:BlockLayoutRow>
				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternA sapUiDemokitLandingBlockHeadline"
						title="Choose Your Scenario"
						titleAlignment="Center"/>
			</l:BlockLayoutRow>

			<l:BlockLayoutRow>
				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternD">
					<l:VerticalLayout
							width="100%">
						<custom:TitleLink
								href="https://www.sap.com/developer/tutorials/hcp-create-trial-account.html"
								text="SAPUI5 on SAP Cloud Platform"
								titleStyle="H2"
								target="_blank"
								class="sapUiSmallMarginBottom"/>
						<Text
								text="Sign up for the free SAP Cloud Platform developer edition to test the platform and build cloud-based UI5 apps."
								class="sapUiSmallMarginBottom"/>
						<l:HorizontalLayout>
							<core:Icon
									src="sap-icon://accept"
									size="0.875rem"
									color="Default"
									class="sapUiTinyMarginBegin sapUiSmallMarginEnd"/>
							<Text text="Easy-to-use development environment with SAP Web IDE"/>
						</l:HorizontalLayout>
						<l:HorizontalLayout class="sapUiSmallMarginBottom">
							<core:Icon
									src="sap-icon://accept"
									size="0.875rem"
									color="Default"
									class="sapUiTinyMarginBegin sapUiSmallMarginEnd"/>
							<Text text="Free trial for development use"/>
						</l:HorizontalLayout>
					</l:VerticalLayout>
				</l:BlockLayoutCell>

				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternC">
					<l:VerticalLayout
							width="100%">
						<custom:TitleLink
								href="http://TODO"
								text="SAPUI5 On-Premise"
								target="_blank"
								titleStyle="H2"
								class="sapUiSmallMarginBottom"/>
						<Text
								text="SAPUI5 is part of the user interface technologies in SAP NetWeaver ABAP or Java systems."
								class="sapUiSmallMarginBottom"/>

						<l:HorizontalLayout>
							<core:Icon
									src="sap-icon://accept"
									size="0.875rem"
									color="Default"
									class="sapUiTinyMarginBegin sapUiSmallMarginEnd"/>
							<Text text="ready to use for SAP Customers"/>
						</l:HorizontalLayout>
						<l:HorizontalLayout class="sapUiSmallMarginBottom">
							<core:Icon
									src="sap-icon://accept"
									size="0.875rem"
									color="Default"
									class="sapUiTinyMarginBegin sapUiSmallMarginEnd"/>
							<Text text="Integrates with existing system landscapes"/>
						</l:HorizontalLayout>

					</l:VerticalLayout>
				</l:BlockLayoutCell>

				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternB">
					<l:VerticalLayout
							width="100%">
						<custom:TitleLink
								href="http://openui5.org"
								text="OpenUI5 - Free and Open Source"
								target="_blank"
								titleStyle="H2"
								class="sapUiSmallMarginBottom"/>
						<Text
								text="Try the open source variant of UI5. It contains all core UI5 features and is available on GitHub under the Apache 2.0 license."
								class="sapUiSmallMarginBottom"/>

						<l:HorizontalLayout>
							<core:Icon
									src="sap-icon://accept"
									size="0.875rem"
									color="Default"
									class="sapUiTinyMarginBegin sapUiSmallMarginEnd"/>
							<Text text="Supported by the open source community"/>
						</l:HorizontalLayout>
						<l:HorizontalLayout class="sapUiSmallMarginBottom">
							<core:Icon
									src="sap-icon://accept"
									size="0.875rem"
									color="Default"
									class="sapUiTinyMarginBegin sapUiSmallMarginEnd"/>
							<Text text="Managed on GitHub - contributions welcome!"/>
						</l:HorizontalLayout>

					</l:VerticalLayout>
				</l:BlockLayoutCell>

				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternC">
					<l:VerticalLayout
							width="100%">
						<custom:TitleLink
								href="#/topic/2d3eb2f322ea4a82983c1c62a33ec4ae"
								text="UI5 on Content Delivery Network"
								titleStyle="H2"
								class="sapUiSmallMarginBottom"/>
						<Text
								text="Develop and run UI5 apps with the flexibility of a robust CDN. Files are loaded from the location closest to the user for optimal performance."
								class="sapUiSmallMarginBottom"/>

						<l:HorizontalLayout>
							<core:Icon
									src="sap-icon://accept"
									size="0.875rem"
									color="Default"
									class="sapUiTinyMarginBegin sapUiSmallMarginEnd"/>
							<Text text="Multi-version CDN to maintain a custom lifecycle"/>
						</l:HorizontalLayout>
						<l:HorizontalLayout class="sapUiSmallMarginBottom">
							<core:Icon
									src="sap-icon://accept"
									size="0.875rem"
									color="Default"
									class="sapUiTinyMarginBegin sapUiSmallMarginEnd"/>
							<Text text="For any on-premise and cloud development scenario"/>
						</l:HorizontalLayout>

					</l:VerticalLayout>
				</l:BlockLayoutCell>

			</l:BlockLayoutRow>

			<l:BlockLayoutRow>
				<l:BlockLayoutCell
						class="demokitInitialPagesColorPatternA sapUiDemokitLandingBlockHeadline"
						title="Join the Community"
						titleAlignment="Center"/>
			</l:BlockLayoutRow>

			<l:BlockLayoutRow>
				<l:BlockLayoutCell
						class="sapUiBlockCenteredCell demokitInitialPagesColorPatternB communityLinkCell">
					<l:HorizontalLayout>
						<core:Icon
								src="sap-icon://collaborate"
								size="2.5rem"
								color="Default"/>
						<custom:TitleLink
								href="https://www.sap.com/community/tag.html?id=sapui5"
								target="_blank"
								text="SAP Community"
								titleStyle="H2"
								wrap="false"
								class="sapUiSmallMargin sapUiNoMarginBottom"/>
					</l:HorizontalLayout>
				</l:BlockLayoutCell>

				<l:BlockLayoutCell
						class="sapUiBlockCenteredCell demokitInitialPagesColorPatternC communityLinkCell">
					<l:HorizontalLayout>
						<Image
								src="./resources/sap/ui/documentation/sdk/images/logo_stackoverflow.png"
								width="2.5rem"
								class="sapUiTinyMarginTop"/>
						<custom:TitleLink
								href="http://stackoverflow.com/search?q=sapui5"
								target="_blank"
								text="StackOverflow"
								titleStyle="H2"
								wrap="false"
								class="sapUiSmallMargin sapUiNoMarginBottom"/>
					</l:HorizontalLayout>
				</l:BlockLayoutCell>

				<l:BlockLayoutCell
						class="sapUiBlockCenteredCell demokitInitialPagesColorPatternD communityLinkCell">
					<l:HorizontalLayout>
						<Image
								src="./resources/sap/ui/documentation/sdk/images/logo_youtube.png"
								width="2.5rem"
								class="sapUiTinyMarginTop"/>
						<custom:TitleLink
								href="https://youtube.com/openui5videos"
								target="_blank"
								text="YouTube"
								titleStyle="H2"
								wrap="false"
								class="sapUiSmallMargin sapUiNoMarginBottom"/>
					</l:HorizontalLayout>
				</l:BlockLayoutCell>

				<l:BlockLayoutCell
						class="sapUiBlockCenteredCell demokitInitialPagesColorPatternE communityLinkCell">
					<l:HorizontalLayout>
						<Image
								src="./resources/sap/ui/documentation/sdk/images/logo_twitter.png"
								width="2.5rem"
								class="sapUiTinyMarginTop"/>
						<custom:TitleLink
								href="https://twitter.com/@openui5"
								target="_blank"
								text="Twitter"
								titleStyle="H2"
								wrap="false"
								class="sapUiSmallMargin sapUiNoMarginBottom"/>
					</l:HorizontalLayout>
				</l:BlockLayoutCell>
			</l:BlockLayoutRow>


			<l:BlockLayoutRow>
				<core:Fragment id="footerFragment" fragmentName="sap.ui.documentation.sdk.view.Footer" type="XML" />
			</l:BlockLayoutRow>

		</l:BlockLayout>
	</Page>
</mvc:View>