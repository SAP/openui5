<mvc:View
		controllerName="sap.ui.documentation.sdk.controller.SampleNotFound"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.m">

	<IllustratedMessage
			id="page"
			illustrationType="sapIllus-PageNotFound"
			title="{i18n>NOT_FOUND_SAMPLE_TITLE}"
			description="{i18n>NOT_FOUND_SAMPLE_TEXT}">
			<additionalContent>
				<VBox>
					<Button
						visible="{device>/system/phone}"
						text="{i18n>NOT_FOUND_BACK}"
						press="onNavBack"
						type="Emphasized"/>
					<Button
						text="{i18n>NOT_FOUND_SAMPLE_NAV_TO_SAMPLES}"
						press="onNavToSamples"
						type="Emphasized"/>
				</VBox>
			</additionalContent>
	</IllustratedMessage>

</mvc:View>
