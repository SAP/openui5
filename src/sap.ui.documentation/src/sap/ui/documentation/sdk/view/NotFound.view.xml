<mvc:View
		controllerName="sap.ui.documentation.sdk.controller.NotFound"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.m">

	<IllustratedMessage
			id="page"
			illustrationType="sapIllus-PageNotFound"
			title="{i18n>NOT_FOUND_TITLE}"
			description="{i18n>NOT_FOULD_TEXT}">
			<additionalContent>
				<Button
						visible="{device>/system/phone}"
						text="{i18n>NOT_FOUND_BACK}"
						press="onNavBack"
						type="Emphasized"/>
			</additionalContent>
	</IllustratedMessage>

</mvc:View>
