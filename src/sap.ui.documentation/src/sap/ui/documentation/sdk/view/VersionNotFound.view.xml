<mvc:View
		controllerName="sap.ui.documentation.sdk.controller.VersionNotFound"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:f="sap.f"
		xmlns="sap.m">

	<App>
		<Page>
			<customHeader>
				<f:ShellBar
					title="HTTP Status 404 - Not Found"
					homeIcon="/resources/sap/ui/documentation/sdk/images/logo_ui5.png"
					homeIconPressed="onHomePress"
				>
				</f:ShellBar>
			</customHeader>
			<f:IllustratedMessage
				id="page"
				illustrationType="sapIllus-PageNotFound"
				title=" "
				description=" "
				busy="true">
				<f:additionalContent>
					<Button
						id="readMoreButton"
						visible="false"
						text="Read More"
						press="onReadMore"
						type="Emphasized" />
					<Button
						id="homePageButton"
						visible="true"
						text="Home Page"
						press="onHomePress" />
				</f:additionalContent>
			</f:IllustratedMessage>
		</Page>
	</App>

</mvc:View>
