<mvc:View
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.uxap"
		xmlns:m="sap.m"
		xmlns:html="http://www.w3.org/1999/xhtml"
		height="100%">

	<ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="false" subSectionLayout="TitleOnLeft">
		<headerTitle>
			<ObjectPageHeader id="objectPageHeader" objectTitle="Header title" />
		</headerTitle>
		<sections>
			<ObjectPageSection id="testSection" title="Anchor1">
				<ObjectPageSubSection id="testSubSection" title="Anchor summary">
					<html:div style="height:300px; background-color: #DFF0FA;"></html:div>
				</ObjectPageSubSection>
			</ObjectPageSection>
		</sections>
		<footer>
			<m:OverflowToolbar>
				<m:ToolbarSpacer />
				<m:Button type="Accept" text="Accept"/>
				<m:Button type="Reject" text="Reject"/>
			</m:OverflowToolbar>
		</footer>
	</ObjectPageLayout>
</mvc:View>
