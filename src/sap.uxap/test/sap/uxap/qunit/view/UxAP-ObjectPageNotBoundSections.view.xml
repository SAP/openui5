<mvc:View
	xmlns="sap.m"
	xmlns:u="sap.uxap"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:mvc="sap.ui.core.mvc">
	<u:ObjectPageLayout 
		id="PartProfileObjectPage" 
		enableLazyLoading="false" 
		showAnchorBarPopover="false" 
		alwaysShowContentHeader="true"
		flexEnabled="true"
		upperCaseAnchorBar="false"
		selectedSection="testSection1">
		
		<u:sections>
			<u:ObjectPageSection 
				id="testSection1"
				title="{
					path : 'ProjectCount',
					formatter : '.formatProjectSectionTitle'
				}"
				titleUppercase="false">
				
				<u:subSections>
					
				</u:subSections>
			</u:ObjectPageSection>

			<u:ObjectPageSection
				id="testSection2"
				titleUppercase="false">
				<u:subSections>
					
					<u:ObjectPageSubSection>
						<u:blocks>
							<l:Grid defaultSpan="XL12 L12 M12 S12">
								<Title
									level="H1"
									text="test"
									class="sapUiTinyMarginBottom"/>
							</l:Grid>
						</u:blocks>
					</u:ObjectPageSubSection>

				</u:subSections>
			</u:ObjectPageSection>
			
		</u:sections>
	</u:ObjectPageLayout>
</mvc:View>