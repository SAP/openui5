<mvc:View
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.uxap"
		xmlns:layout="sap.ui.layout"
		xmlns:m="sap.m"
		height="100%">
	<ObjectPageHeaderContent id="ObjectPageHeaderContent">
		<content>
			<layout:VerticalLayout>
				<m:Link text="denise-smith"/>
				<m:Label text="(321) 123-4567"/>
				<m:Link text="DeniseSmith@sap.com"/>
			</layout:VerticalLayout>
			<m:Text width="200px"
					text="Hi, I'm Denise. I am passionate about what I do and I'll go the extra mile to make the customer win."/>
			<layout:VerticalLayout>
				<m:Label text="Profile completion"/>
				<m:ProgressIndicator
						percentValue="30"
						displayValue="30%"
						showValue="true"
						state="None"/>
			</layout:VerticalLayout>
		</content>
	</ObjectPageHeaderContent>

	<ObjectPageLayout id="ObjectPageLayout"
					  showTitleInHeaderContent="true">
		<headerTitle>
			<ObjectPageHeader id="headerForTest"
							  objectTitle="Denise Smith"
							  objectSubtitle="Senior Developer">
			</ObjectPageHeader>
		</headerTitle>
		<headerContent>
			<m:Link text="denise-smith" id="testLink"/>
			<layout:VerticalLayout>
				<m:Label text="(321) 123-4567"/>
				<m:Link text="DeniseSmith@sap.com"/>
				<layout:HorizontalLayout>
					<m:Image height="24px" width="24px" src="test-resources/sap/uxap/qunit/img/twitterIcon.png"/>
					<m:Image height="24px" width="24px" src="test-resources/sap/uxap/qunit/img/linkedInIcon.png"/>
				</layout:HorizontalLayout>
			</layout:VerticalLayout>
			<m:Text width="200px"
					text="Hi, I'm Denise. I am passionate about what I do and I'll go the extra mile to make the customer win."/>
			<layout:VerticalLayout>
				<m:Label text="Profile completion"/>
				<m:ProgressIndicator
						percentValue="30"
						displayValue="30%"
						showValue="true"
						state="None"/>
			</layout:VerticalLayout>
		</headerContent>
	</ObjectPageLayout>
</mvc:View>
