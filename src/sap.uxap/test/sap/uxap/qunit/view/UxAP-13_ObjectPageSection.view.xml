<mvc:View
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.uxap"
		xmlns:sample="sap.uxap.testblocks.multiview"
		xmlns:m="sap.m"
		xmlns:html="http://www.w3.org/1999/xhtml"
		height="100%">

	<ObjectPageLayout id="ObjectPageLayout">
		<sections>
			<ObjectPageSection id="SectionWithSubSection" title="My first section title with subsections">

				<subSections>
					<ObjectPageSubSection id="subsection1" title="My first subSection Title" class="mysubsectiontotest">
						<blocks>
							<sample:MultiViewBlock/>
						</blocks>
					</ObjectPageSubSection>
					<ObjectPageSubSection id="subsection2" title="My second subSection Title"
										  class="mysubsectiontotest">
						<blocks>
							<sample:MultiViewBlock/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>

			<ObjectPageSection id="SectionWithoneSubSection" title="My second section title with one subsection">
				<subSections>
					<ObjectPageSubSection id="subsection3" title="My third subSection Title" class="mysubsectiontotest">
						<blocks>
							<sample:MultiViewBlock/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>

			<ObjectPageSection id="SectionWithoutSubSection" title="My third section title without subsection">

			</ObjectPageSection>

			<ObjectPageSection id="SectionWithNoTitleAndTwoSubSections">
				<subSections>
					<ObjectPageSubSection id="subsection4" title="My first SubSection title" class="mysubsectiontotest">
						<blocks>
							<sample:MultiViewBlock/>
						</blocks>
					</ObjectPageSubSection>
					<ObjectPageSubSection id="subsection5" title="My second SubSection title" class="mysubsectiontotest">
						<blocks>
							<sample:MultiViewBlock/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>

			<ObjectPageSection id="SectionWithTwoSubsectionsWithoutTitles" title="Section with subsections without titles">
				<subSections>
					<ObjectPageSubSection id="subsection6">
						<blocks>
							<sample:MultiViewBlock/>
						</blocks>
					</ObjectPageSubSection>
					<ObjectPageSubSection id="subsection7">
						<blocks>
							<sample:MultiViewBlock/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>

			<ObjectPageSection id="SectionWithNoTitleAndOneSubSection">
				<subSections>
					<ObjectPageSubSection id="subsection8" title="My last subSection Title" class="mysubsectiontotest">
						<blocks>
							<sample:MultiViewBlock/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>


		</sections>

	</ObjectPageLayout>

</mvc:View>
