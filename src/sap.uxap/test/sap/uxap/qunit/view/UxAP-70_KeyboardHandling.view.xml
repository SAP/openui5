					<ObjectPageSubSection title="SubSection7">
						<html:div style="height:300px; background-color: #DFF0FA;"></html:div>
					</ObjectPageSubSection>

					<ObjectPageSubSection title="Employee Details" mode="Collapsed">
						<html:div style="height:300px; background-color: #DFF0FA;"></html:div>
					</ObjectPageSubSection>

					<ObjectPageSubSection title="Job Relationship" mode="Collapsed">
						<html:div style="height:300px; background-color: #DFF0FA;"></html:div>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection id="section-with-single-sub-section" title="Section with Single SubSection">
				<subSections>
					<ObjectPageSubSection id="single-sub-section" title="Goal 1">
						<actions>
							<m:Button id="single-subsection-show-section" icon="sap-icon://pull-down" text="show section"/>
							<m:Link text="denise-smith"/>
						</actions>
						<blocks>
							<m:Label text="lbl with txt"/>
							<m:Link text="denise-smith"/>
							<m:Link text="lbl with other txt"/>
							<m:Button id="interactive-el-single-sub-section" icon="sap-icon://pull-down"
									  text="Interactive Element"/>
							<m:Input id="input-single-sub-section"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection id="section-with-multiple-sub-section" title="Section with Multiple SubSection">
				<subSections>
					<ObjectPageSubSection id="multiple-sub-section-1" title="Goal 1">
						<actions>
							<m:Button id="button-toolbar" icon="sap-icon://pull-down" text="show section"/>
							<m:Link text="denise-smith"/>
						</actions>
						<blocks>
							<m:Label text="lbl with txt"/>
							<m:Link text="denise-smith"/>
							<m:Link text="lbl with other txt"/>
							<m:Button icon="sap-icon://pull-down" text="Interactive Element"/>
							<m:Input id="input-multiple-sub-section"/>
						</blocks>
					</ObjectPageSubSection>
					<ObjectPageSubSection id="multiple-sub-section-2" title="Goal 1">
						<blocks>
							<m:Label text="lbl with txt"/>
							<m:Link id="first-focusable-subsection-2" text="denise-smith"/>
							<m:Link text="lbl with other txt"/>
							<m:Button id="interactive-el-multiple-sub-section" icon="sap-icon://pull-down"
									  text="Interactive Element"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</mvc:View>
