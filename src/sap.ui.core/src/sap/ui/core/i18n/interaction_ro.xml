<?xml version="1.0" encoding="UTF8"?>
<interaction-documentation library="sap.ui.core" xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1">
	<control-interactions>
		<control name="sap.ui.core.Control"/>
		<interaction> <kbd>Tab</kbd> <description>
				<p>Mută focalizarea la următorul element din lanțul de taburi.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + Tab</kbd> <description>
				<p>Mută focalizarea la elementul anterior din lanțul de taburi.</p>
			</description>
		</interaction>
		<interaction> <kbd>F6</kbd> <description>
				<p>Mută focalizarea la primul element al următorului grup UI (grup F6) din lanțul de taburi.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + F6</kbd> <description>
				<p>Mută focalizarea la primul element al grupului UI anterior (grup F6) din lanțul de taburi.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.core.mvc.XMLView">
			<defaultLabel>Shortcuturi globale</defaultLabel>
		</control>
	</control-interactions>
</interaction-documentation>