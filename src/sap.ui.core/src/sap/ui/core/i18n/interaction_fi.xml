<?xml version="1.0" encoding="UTF8"?>
<interaction-documentation library="sap.ui.core" xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1">
	<control-interactions>
		<control name="sap.ui.core.Control"/>
		<interaction> <kbd>Tab</kbd> <description>
				<p>Siirtää kohdistuksen seuraavaan elementtiin sarkainjärjestyksessä.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + Tab</kbd> <description>
				<p>Siirtää kohdistuksen edelliseen elementtiin sarkainjärjestyksessä.</p>
			</description>
		</interaction>
		<interaction> <kbd>F6</kbd> <description>
				<p>Siirtää kohdistuksen seuraavan UI-ryhmän (F6-ryhmä) ensimmäiseen elementtiin sarkainjärjestyksessä.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + F6</kbd> <description>
				<p>Siirtää kohdistuksen edellisen UI-ryhmän (F6-ryhmä) ensimmäiseen elementtiin sarkainjärjestyksessä.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.core.mvc.XMLView">
			<defaultLabel>Yleiset pikavalinnat</defaultLabel>
		</control>
	</control-interactions>
</interaction-documentation>