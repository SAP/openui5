<?xml version="1.0" encoding="UTF8"?>
<interaction-documentation library="sap.ui.core" xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1">
	<control-interactions>
		<control name="sap.ui.core.Control"/>
		<interaction> <kbd>Tab</kbd> <description>
				<p>Flyttar fokus till nästa element i tabulatorföljden.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + Tab</kbd> <description>
				<p>Flyttar fokus till föregående element i tabulatorföljden.</p>
			</description>
		</interaction>
		<interaction> <kbd>F6</kbd> <description>
				<p>Flyttar fokus till första elementet i nästa UI-grupp (F6-grupp) i tabulatorföljden.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + F6</kbd> <description>
				<p>Flyttar fokus till första elementet i föregående UI-grupp (F6-grupp) i tabulatorföljden.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.core.mvc.XMLView">
			<defaultLabel>Globala genvägar</defaultLabel>
		</control>
	</control-interactions>
</interaction-documentation>