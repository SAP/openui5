<?xml version="1.0" encoding="UTF8"?>
<interaction-documentation library="sap.ui.core" xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1">
	<control-interactions>
		<control name="sap.ui.core.Control"/>
		<interaction> <kbd>Tab</kbd> <description>
				<p>Desplaça el focus a l'element següent de la cadena de pestanyes.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + Tab</kbd> <description>
				<p>Desplaça el focus a l'element anterior de la cadena de pestanyes.</p>
			</description>
		</interaction>
		<interaction> <kbd>F6</kbd> <description>
				<p>Desplaça el focus al primer element del grup d'IU següent (grup F6) de la cadena de pestanyes.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + F6</kbd> <description>
				<p>Desplaça el focus al primer element del grup d'IU anterior (grup F6) de la cadena de pestanyes.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.core.mvc.XMLView">
			<defaultLabel>Dreceres globals</defaultLabel>
		</control>
	</control-interactions>
</interaction-documentation>