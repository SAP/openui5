<?xml version="1.0" encoding="UTF8"?>
<interaction-documentation library="sap.ui.core" xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1">
	<control-interactions>
		<control name="sap.ui.core.Control"/>
		<interaction> <kbd>Tab</kbd> <description>
				<p>Verplaatst de focus naar het volgende element in de tabbladketen.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + Tab</kbd> <description>
				<p>Verplaatst de focus naar het vorige element in de tabbladketen.</p>
			</description>
		</interaction>
		<interaction> <kbd>F6</kbd> <description>
				<p>Verplaatst de focus naar het eerste element van de volgende UI-groep (F6-groep) in de tabbladketen.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + F6</kbd> <description>
				<p>Verplaatst de focus naar het eerste element van de vorige UI-groep (F6-groep) in de tabbladketen.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.core.mvc.XMLView">
			<defaultLabel>Globale snelkoppelingen</defaultLabel>
		</control>
	</control-interactions>
</interaction-documentation>