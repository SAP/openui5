					<PropertyRef Name="Key/P1" Alias="Key1"/>
					<PropertyRef Name="Key/P2" Alias="Key2"/>
				</Key>
				<Property Name="Key" Type="special.cases.ComplexKey"/>
				<Property Name="Value" Type="Edm.String"/>
