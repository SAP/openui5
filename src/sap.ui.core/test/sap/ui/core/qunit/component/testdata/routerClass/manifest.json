{
	"_version": "2.0.0",
	"sap.app": {
		"id": "testdata.routerClass",
		"type": "application",
		"title": "Application 'routerClass'",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"rootView": "testdata.routerClass.Main",
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router"
			},
			"routes": [
				{
					"name": "firstRoute",
					"pattern": "first/{firstMandatoryParameter}"
				}
			],
			"targets": {
				"myTarget": {
					"viewType": "XML"
				}
			}
		}
	},
	"sap.ui.generic.app": {
		"settings": {
			"flexibleColumnLayout": true
		}
	}
}