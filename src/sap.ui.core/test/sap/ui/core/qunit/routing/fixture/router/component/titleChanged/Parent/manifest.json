{
	"_version": "2.0.2",
	"sap.app": {
		"id": "qunit.router.component.titleChanged.Parent",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"type": "application",
		"title": "Application 'Parent'"
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": "qunit.router.component.titleChanged.Parent.view.RootView",
		"routing": {
			"config": {
				"controlAggregation": "content",
				"controlId": "rootPanel",
				"viewType": "XML",
				"type": "View",
				"path": "qunit.router.component.titleChanged.Parent.view"
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": "home"
				},
				{
					"pattern": "second",
					"name": "second",
					"target": "second"
				},
				{
					"pattern": "third",
					"name": "third",
					"target": {
						"name": "third",
						"prefix": "child",
						"propagateTitle": true
					}
				}
			],
			"targets": {
				"home": {},
				"view1": {
					"name": "MyView1",
					"title": "home"
				},
				"second": {
					"name": "MyView2",
					"title": "{/secondViewTitle}"
				},
				"third": {
					"name": "Child",
					"path": "qunit.router.component.titleChanged",
					"type": "Component",
					"id": "childComponent",
					"title": "ChildComponentTitle",
					"options": {
						"manifest": false
					}
				}
			}
		}
	}
}