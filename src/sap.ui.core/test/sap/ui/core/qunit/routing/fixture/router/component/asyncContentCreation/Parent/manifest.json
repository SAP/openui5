{
	"_version": "2.0.2",
	"sap.app": {
		"id": "qunit.router.component.asyncContentCreation.Parent",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"type": "application",
		"title": "Application 'Parent'"
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": "qunit.router.component.asyncContentCreation.Parent.view.RootView",
		"routing": {
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": [
						{
							"name": "home",
							"prefix": "child"
						},
						"footer"
					]
				},
				{
					"pattern": "category",
					"name": "category"
				}
			],
			"targets": {
				"home": {
					"name": "Child",
					"path": "qunit.router.component.asyncContentCreation",
					"type": "Component",
					"clearControlAggregation": true,
					"controlId": "shell",
					"controlAggregation": "content"
				},
				"footer": {
					"name": "Footer",
					"viewType": "XML",
					"type": "View",
					"path": "qunit.router.component.asyncContentCreation.Parent.view",
					"controlId": "box",
					"controlAggregation": "items"
				}
			}
		}
	}
}