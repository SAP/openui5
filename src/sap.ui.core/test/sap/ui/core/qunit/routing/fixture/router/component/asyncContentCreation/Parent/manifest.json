{
	"sap.app": {
		"id": "qunit.router.component.asyncContentCreation.Parent",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "qunit.router.component.asyncContentCreation.Parent.view.RootView",
			"type": "XML",
			"async": true
		},
		"routing": {
			"config": {
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": [{
						"name": "home",
						"prefix": "child"
					}, "footer"]
				},
				{
					"pattern": "category",
					"name": "category"
				}
			],
			"targets": {
				"home": {
					"name": "Child",
					"path": "qunit.router.component.asyncContentCreation",
					"type": "Component",
					"clearControlAggregation": true,
					"controlId": "shell",
					"controlAggregation": "content"
				},
				"footer": {
					"name": "Footer",
					"viewType": "XML",
					"type": "View",
					"path": "qunit.router.component.asyncContentCreation.Parent.view",
					"controlId": "box",
					"controlAggregation": "items"
				}
			}
		}
	}
}
