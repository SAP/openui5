{
	"_version": "2.0.0",
	"sap.app": {
		"dataSources": {
			"default": {
				"uri": "/foo/bar/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		},
		"id": "samples.components.sample.config.modelPreloadAndEarlyRequests",
		"type": "application",
		"title": "Application 'modelPreloadAndEarlyRequests'",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.53"
		},
		"models": {
			"": {
				"dataSource": "default",
				"preload": true,
				"settings": {
					"earlyRequests": false,
					"operationMode": "Server"
				}
			},
			"without earlyRequests": {
				"dataSource": "default",
				"preload": true,
				"settings": {
					"operationMode": "Server"
				}
			}
		}
	}
}