<mvc:View
	controllerName="sap.ui.core.sample.Messaging.ODataBackendMessagesComp.controller.NotFound"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<IllustratedMessage
		illustrationType="sapIllus-PageNotFound"
		title="{i18n>NotFound.title}"
		description="{i18n>NotFound.description}">
		<additionalContent>
			<Button text="{i18n>NotFound.back}" press="onNavBack" />
		</additionalContent>
	</IllustratedMessage>
</mvc:View>
