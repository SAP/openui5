{
	"_version": "2.0.2",
	"sap.app": {
		"applicationVersion": {
			"version": "${version}"
		},
		"description": "Example how to use OData V4 Edm.TimeOfDay in SAPUI5 applications",
		"dataSources": {
			"default": {
				"type": "JSON"
			}
		},
		"id": "sap.ui.core.sample.odata.types.v4.TimeOfDay",
		"type": "application",
		"title": "Application 'TimeOfDay'"
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"config": {
			"sample": {
				"files": [
					"TimeOfDay.view.xml",
					"TimeOfDay.controller.js",
					"manifest.json",
					"Component.js"
				]
			}
		},
		"dependencies": {
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"handleValidation": true,
		"models": {
			"": {
				"dataSource": "default",
				"type": "sap.ui.model.json.JSONModel"
			}
		},
		"rootView": "sap.ui.core.sample.odata.types.v4.TimeOfDay.TimeOfDay"
	}
}