{
	"_version" : "1.14.0",
	"sap.app" : {
		"ach" : "CA-UI5-COR",
		"applicationVersion" : {
			"version" : "${version}"
		},
		"description" : "Example for OData V4 EPM Products",
		"dataSources" : {
			"default" : {
				"uri" : "/sap/opu/odata4/sap/zui5_testv4/default/sap/zui5_epm_sample/0002/?custom-option=value",
				"type" : "OData",
				"settings" : {
					"annotations": ["localAnnotations"],
					"odataVersion" : "4.0"
				}
			},
			"localAnnotations": {
				"uri": "data/localAnnotations.xml",
				"type": "ODataAnnotation"
			}
		},
		"id" : "sap.ui.core.sample.odata.v4.Products",
		"title" : "EPM Products",
		"type" : "application"
	},
	"sap.ui" : {
		"deviceTypes" : {
			"desktop" : true,
			"phone" : true,
			"tablet" : true
		},
		"technology" : "UI5"
	},
	"sap.ui5" : {
		"config" : {
			"sample" : {
				"files" : [
					"Component.js",
					"Main.controller.js",
					"Main.view.xml",
					"manifest.json",
					"SandboxModel.js",
					"../../../common/Controller.js",
					"../../../common/SandboxModelHelper.js"
				]
			}
		},
		"contentDensities" : {
			"compact" : true,
			"cozy" : true
		},
		"dependencies" : {
			"minUI5Version" : "1.63",
			"libs" : {
				"sap.m" : {}
			}
		},
		"handleValidation" : true,
		"models" : {
			"" : {
				"dataSource" : "default",
				"preload" : true,
				"settings" : {
					"autoExpandSelect" : true,
					"operationMode" : "Server",
					"synchronizationMode" : "None"
				},
				"type" : "sap.ui.core.sample.odata.v4.Products.SandboxModel",
				"uriSettingName" : "serviceUrl"
			}
		}
	}
}
