{
	"_version": "1.17.0",
	"sap.app": {
		"id": "sap.ui.core.sample.RoutingNestedComponent.reuse.products",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"northwind": {
				"uri": "https://cors-anywhere.herokuapp.com/https://services.odata.org/V2/Northwind/Northwind.svc",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.core.sample.RoutingNestedComponent.reuse.products.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.72",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.core.sample.RoutingNestedComponent.reuse.products.i18n.i18n"
				}
			},
			"": {
				"dataSource": "northwind",
				"defaultBindingMode": "TwoWay",
				"preload": true,
				"settings": {
					"useBatch": false,
					"defaultCountMode": "Inline"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"path": "sap.ui.core.sample.RoutingNestedComponent.reuse.products.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [
				{
					"name": "list",
					"pattern": ":basepath:",
					"target": "list"
				},
				{
					"name": "detail",
					"pattern": "detail/{id}",
					"target": "detail"
				}
			],
			"targets": {
				"list": {
					"type": "View",
					"id": "list",
					"name": "List",
					"title": "Products List"
				},
				"detail": {
					"type": "View",
					"id": "detail",
					"name": "Detail",
					"title": "{ProductName}"
				},
				"notFound": {
					"type": "View",
					"id": "notFound",
					"name": "NotFound",
					"transition": "show"
				}
			}
		}
	}
}
