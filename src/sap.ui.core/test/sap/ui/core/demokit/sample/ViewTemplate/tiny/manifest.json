{
	"_version": "2.0.2",
	"sap.app": {
		"ach": "CA-UI5-COR",
		"applicationVersion": {
			"version": "${version}"
		},
		"description": "Minimal sample for XML Templating. Requires a properly configured proxy.",
		"id": "sap.ui.core.demokit.sample.ViewTemplate.tiny",
		"title": "XML Templating: minimal sample",
		"type": "application"
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"config": {
			"sample": {
				"files": [
					"annotations.xml",
					"Component.js",
					"Field.fragment.xml",
					"manifest.json",
					"package.json",
					"Template.view.xml",
					"ui5.yaml"
				]
			}
		},
		"dependencies": {
			"minUI5Version": "1.120",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		}
	}
}