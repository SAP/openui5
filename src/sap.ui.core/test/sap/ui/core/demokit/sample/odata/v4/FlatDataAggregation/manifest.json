{
	"_version": "2.0.2",
	"sap.app": {
		"ach": "CA-UI5-COR",
		"applicationVersion": {
			"version": "${version}"
		},
		"dataSources": {
			"default": {
				"uri": "/analytics/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		},
		"description": "Example for OData V4 Data Aggregation w/o visual grouping; based on https://github.com/SAP-samples/cap-sflight",
		"id": "sap.ui.core.sample.odata.v4.FlatDataAggregation",
		"title": "OData V4 Flat Data Aggregation",
		"type": "application"
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"config": {
			"sample": {
				"files": [
					"Component.js",
					"FlatDataAggregation.controller.js",
					"FlatDataAggregation.view.xml",
					"manifest.json",
					"SandboxModel.js",
					"../../../common/SandboxModelHelper.js"
				]
			}
		},
		"dependencies": {
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.table": {}
			},
			"minUI5Version": "1.129.0"
		},
		"handleValidation": true,
		"models": {
			"": {
				"dataSource": "default",
				"preload": true,
				"settings": {
					"autoExpandSelect": false,
					"earlyRequests": true,
					"operationMode": "Server"
				}
			}
		},
		"rootView": "sap.ui.core.sample.odata.v4.FlatDataAggregation.FlatDataAggregation"
	}
}