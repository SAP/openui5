{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"ach": "CA-UI5-COR",
		"applicationVersion": {
			"version": "${version}"
		},
		"description": "{{sap.app.description}}",
		"id": "sap.ui.core.sample.odata.v4.SalesOrdersTemplate",
		"i18n": "i18n/i18n.properties",
		"title": "{{sap.app.title}}",
		"type": "application"
	},
	"sap.ui": {
		"_version": "1.1.0",
		"deviceTypes": {
			"desktop": true,
			"phone": true,
			"tablet": true
		},
		"supportedThemes": [
			"sap_bluecrystal",
			"sap_hcb"
		],
		"technology": "UI5"
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"config": {
			"sample": {
				"files": [
					"Component.js",
					"Main.view.xml",
					"../../../common/Component.js"
				]
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"dependencies": {
			"minUI5Version": "1.34",
			"libs": {
				"sap.m": {}
			}
		},
		"handleValidation": false,
		"rootView": "Main.view.xml"
	}
}