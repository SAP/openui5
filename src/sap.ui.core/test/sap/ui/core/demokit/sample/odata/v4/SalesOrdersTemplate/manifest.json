{
	"_version": "2.0.0",
	"sap.app": {
		"ach": "CA-UI5-COR",
		"applicationVersion": {
			"version": "${version}"
		},
		"description": "Example for OData V4 EPM Sales Orders using XML Templating",
		"dataSources": {
			"default": {
				"uri": "/sap/opu/odata4/sap/zui5_testv4/default/sap/zui5_epm_sample/0002/",
				"type": "OData",
				"settings": {
					"annotations": [
						"testanno"
					],
					"odataVersion": "4.0"
				}
			},
			"testanno": {
				"uri": "data/annotations.xml",
				"type": "ODataAnnotation"
			}
		},
		"id": "sap.ui.core.sample.odata.v4.SalesOrdersTemplate",
		"title": "EPM Sales Orders Template",
		"type": "application"
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"config": {
			"sample": {
				"files": [
					"Component.js",
					"Facets.fragment.xml",
					"FormattedText.fragment.xml",
					"Helper.js",
					"Main.controller.js",
					"Main.view.xml",
					"manifest.json",
					"SandboxModel.js",
					"data/annotations.xml",
					"data/BusinessPartnerList.json",
					"data/CurrencyList.json",
					"data/metadata_bp_role.xml",
					"data/metadata_tcurc.xml",
					"data/metadata.xml",
					"../../../common/Controller.js",
					"../../../common/ValueHelp.js",
					"../../../common/SandboxModelHelper.js"
				]
			}
		},
		"dependencies": {
			"minUI5Version": "1.37",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"handleValidation": false,
		"models": {
			"": {
				"dataSource": "default",
				"preload": true,
				"settings": {
					"earlyRequests": true
				},
				"type": "sap.ui.core.sample.odata.v4.SalesOrdersTemplate.SandboxModel",
				"uriSettingName": "serviceUrl"
			}
		}
	}
}