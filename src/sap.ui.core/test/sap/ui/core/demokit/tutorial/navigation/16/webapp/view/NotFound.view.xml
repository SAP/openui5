<mvc:View
	controllerName="sap.ui.demo.nav.controller.NotFound"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<IllustratedMessage
		illustrationType="sapIllus-PageNotFound"
		title="{i18n>NotFound}"
		description="{i18n>NotFound.description}">
		<additionalContent>
			<Button text="{i18n>NotFound.backToHome}" press=".onNavBack"></Button>
		</additionalContent>
	</IllustratedMessage>
</mvc:View>
