<?xml version="1.0" encoding="UTF-8" ?>
<library xmlns="http://www.sap.com/sap.ui.library.xsd">

	<name>sap.ui.support</name>
	<vendor>SAP SE</vendor>
	<copyright>${copyright}</copyright>
	<version>${version}</version>

	<documentation>UI5 library: sap.ui.support</documentation>

	<dependencies>
		<dependency>
			<libraryName>sap.ui.core</libraryName>
		</dependency>
	</dependencies>

	<appData>
		<manifest xmlns="http://www.sap.com/ui5/buildext/manifest">
			<sap.ui5>
				<library>
					<i18n>false</i18n><!-- no default i18n bundle in this lib -->
				</library>
			</sap.ui5>
		</manifest>
		<packaging xmlns="http://www.sap.com/ui5/buildext/packaging" version="2.0" >
			<!-- excludes for the all-in-one file -->
			<all-in-one>
				<exclude name="sap/ui/support/supportRules/util/EvalUtils.js" />
			</all-in-one>
		</packaging>
		<!-- excludes for the JSDoc -->
		<jsdoc xmlns="http://www.sap.com/ui5/buildext/jsdoc">
			<exclude name="sap.ui.support.js."/>
		</jsdoc>
		<!-- excludes for the JSCoverage -->
		<jscoverage xmlns="http://www.sap.com/ui5/buildext/jscoverage">
			<exclude name="sap.ui.support.js."/>
		</jscoverage>
	</appData>

</library>
