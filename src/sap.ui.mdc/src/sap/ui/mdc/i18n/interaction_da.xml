<?xml version="1.0" encoding="UTF8"?>
<interaction-documentation library="sap.ui.comp" xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1">
	<control-interactions>
		<control name="sap.ui.mdc.Table"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Åbner dialogen <span class="uicontrol">Tabelindstillinger</span>, hvis den er tilgængelig.</p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + Shift + E</kbd> <description>
				<p>Åbner dialogen <span class="uicontrol">Eksportér som</span> i tabellen, hvis den er tilgængelig.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.FilterBar"/>
		<interaction> <kbd>Enter</kbd> <description>
				<p>Udløser søgning efter filterlinje.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.Chart"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Åbner dialogen <span class="uicontrol">Visningsindstillinger</span>.</p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + D</kbd> <description>
				<p>Sletter et element i dialogen <span class="uicontrol">Visningsindstillinger</span>.</p>
			</description>
		</interaction>
	</control-interactions>
</interaction-documentation>