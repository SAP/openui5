<interaction-documentation
	xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1"
	library="sap.ui.comp">
	<control-interactions>
		<control name="sap.ui.mdc.Table"></control>
		<interaction>
			<kbd>Ctrl + ,</kbd>
			<description>
				<p>Opens the <span class="uicontrol">Table Settings</span> dialog, if available.</p>
			</description>
		</interaction>
		<interaction>
			<kbd>Ctrl + Shift + E</kbd>
			<description>
				<p>Opens the <span class="uicontrol">Export As</span> dialog of the table, if available.</p>
			</description>
		</interaction>
	</control-interactions>
</interaction-documentation>