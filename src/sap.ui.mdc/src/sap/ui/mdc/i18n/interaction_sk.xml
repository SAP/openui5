<?xml version="1.0" encoding="UTF8"?>
<interaction-documentation library="sap.ui.comp" xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1">
	<control-interactions>
		<control name="sap.ui.mdc.Table"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Otvorí dialógové okno <span class="uicontrol">Nastavenia tabuľky</span>, ak je k dispozícii.</p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + Shift + E</kbd> <description>
				<p>Otvorí dialógové okno <span class="uicontrol">Exportovať ako tabuľky</span>, ak je k dispozícii.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.FilterBar"/>
		<interaction> <kbd>Enter</kbd> <description>
				<p>Spustí vyhľadávanie lišty filtrov.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.Chart"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Otvorí dialógové okno <span class="uicontrol">Nastavenia zobrazenia</span></p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + D</kbd> <description>
				<p>Odstráni položku v dialógovom okne <span class="uicontrol">Nastavenia zobrazenia</span></p>
			</description>
		</interaction>
	</control-interactions>
</interaction-documentation>