<?xml version="1.0" encoding="UTF8"?>
<interaction-documentation library="sap.ui.comp" xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1">
	<control-interactions>
		<control name="sap.ui.mdc.Table"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Öppnar dialogen <span class="uicontrol">Inställningar för tabell</span> om den är tillgänglig.</p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + Shift + E</kbd> <description>
				<p>Öppnar dialogen <span class="uicontrol">Exportera som</span> för tabellen om den är tillgänglig.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.FilterBar"/>
		<interaction> <kbd>Enter</kbd> <description>
				<p>Initierar sökning för filterfält.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.Chart"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Öppna dialogen <span class="uicontrol">Vyinställningar</span>.</p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + D</kbd> <description>
				<p>Raderar ett element i dialogen <span class="uicontrol">Vyinställningar</span>.</p>
			</description>
		</interaction>
	</control-interactions>
</interaction-documentation>