<?xml version="1.0" encoding="UTF8"?>
<interaction-documentation library="sap.ui.comp" xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1">
	<control-interactions>
		<control name="sap.ui.mdc.Table"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Odpre pogovorno okno <span class="uicontrol">Nastavitve tabele</span>, če je na voljo.</p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + Shift + E</kbd> <description>
				<p>Odpre pogovorno okno <span class="uicontrol">Izvozi kot</span>, če je na voljo.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.FilterBar"/>
		<interaction> <kbd>Enter</kbd> <description>
				<p>Sproži iskanje vrstice s filtri.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.Chart"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Odpre pogovorno okno <span class="uicontrol">Nastavitve pogleda</span></p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + D</kbd> <description>
				<p>Izbriše element v pogovornem oknu <span class="uicontrol">Nastavitve pogleda</span></p>
			</description>
		</interaction>
	</control-interactions>
</interaction-documentation>