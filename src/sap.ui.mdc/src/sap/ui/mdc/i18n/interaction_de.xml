<?xml version="1.0" encoding="UTF8"?>
<interaction-documentation library="sap.ui.comp" xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1">
	<control-interactions>
		<control name="sap.ui.mdc.Table"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Öffnet den Dialog <span class="uicontrol">Tabelleneinstellungen</span>, falls verfügbar.</p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + Shift + E</kbd> <description>
				<p>Öffnet den Dialog <span class="uicontrol">Exportieren als</span> der Tabelle, falls verfügbar.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.FilterBar"/>
		<interaction> <kbd>Enter</kbd> <description>
				<p>Löst die Suche für die Filterleiste aus.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.Chart"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Opens the <span class="uicontrol">View Settings</span> dialog.</p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + D</kbd> <description>
				<p>Deletes an item in the <span class="uicontrol">View Settings</span> dialog.</p>
			</description>
		</interaction>
	</control-interactions>
</interaction-documentation>