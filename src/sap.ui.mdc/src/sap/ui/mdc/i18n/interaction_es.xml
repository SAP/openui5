<?xml version="1.0" encoding="UTF8"?>
<interaction-documentation library="sap.ui.comp" xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1">
	<control-interactions>
		<control name="sap.ui.mdc.Table"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Se abre el diálogo <span class="uicontrol">Opciones de tabla</span>, si está disponible.</p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + Shift + E</kbd> <description>
				<p>Se abre el diálogo <span class="uicontrol">Exportar como</span> de la tabla, si está disponible.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.FilterBar"/>
		<interaction> <kbd>Enter</kbd> <description>
				<p>Inicia la búsqueda para la barra de filtros.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.Chart"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Abre el diálogo <span class="uicontrol">Ver parametrizaciones</span></p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + D</kbd> <description>
				<p>Elimina un elemento del diálogo <span class="uicontrol">Ver parametrizaciones</span></p>
			</description>
		</interaction>
	</control-interactions>
</interaction-documentation>