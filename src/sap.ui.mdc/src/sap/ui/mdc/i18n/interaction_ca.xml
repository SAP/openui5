<?xml version="1.0" encoding="UTF8"?>
<interaction-documentation library="sap.ui.comp" xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1">
	<control-interactions>
		<control name="sap.ui.mdc.Table"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Obre el diàleg <span class="uicontrol">Opcions de taula</span>, si està disponible.</p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + Shift + E</kbd> <description>
				<p>Obre el diàleg <span class="uicontrol">Exportar com a</span> de la taula, si està disponible.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.FilterBar"/>
		<interaction> <kbd>Enter</kbd> <description>
				<p>Inicia la cerca per a la barra de filtres.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.mdc.Chart"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Obre el diàleg <span class="uicontrol">Opcions de vista</span></p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + D</kbd> <description>
				<p>Suprimeix un element del diàleg <span class="uicontrol">Opcions de vista</span></p>
			</description>
		</interaction>
	</control-interactions>
</interaction-documentation>