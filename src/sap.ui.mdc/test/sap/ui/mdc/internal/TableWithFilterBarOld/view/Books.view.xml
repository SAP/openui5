					</f2:heading>

					<f2:snappedContent>
						<Text id="statusTextCollapsed" />
					</f2:snappedContent>
					<f2:expandedContent>
						<Text id="statusTextExpanded" />
					</f2:expandedContent>
				</f2:DynamicPageTitle>
			</f2:title>
			<f2:header>
