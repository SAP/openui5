<mvc:View controllerName="sap.ui.v4demo.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:tnt="sap.tnt">
	<tnt:ToolHeader>
		<IconTabHeader selectedKey="{app>/routeName}" id="iconTabHeader" select=".onNavigate">
			<items>
				<IconTabFilter key="chart" text="Remake" />
			</items>
		</IconTabHeader>
		<ToolbarSpacer />
		<Button text="Features" icon="sap-icon://settings" type="Emphasized" press="onFeatureToggle"/>
		<ToggleButton text="RTA" icon="sap-icon://user-edit" type="Emphasized" press="onStartRTA" />
	</tnt:ToolHeader>
	<App id="app">
	</App>
</mvc:View>
