{
	"_version": "2.0.2",
	"sap.app": {
		"id": "sap.ui.mdc.demokit.sample.Chart.ChartJS",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "mdc Chart Demo",
		"description": "MDC Chart with Chart.js",
		"dataSources": {
			"Products": {
				"uri": "model/Products.json",
				"type": "JSON"
			},
			"PropertyInfos": {
				"uri": "model/PropertyInfos.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"config": {
			"sample": {
				"files": [
					"manifest.json",
					"view/Demo.view.xml",
					"controller/Demo.controller.js",
					"delegate/ChartJSDelegate.js",
					"delegate/ChartJSPropertyHelper.js",
					"control/ChartWrapper.js",
					"model/Products.json",
					"model/PropertyInfos.json",
					"Component.js",
					"index.html"
				]
			}
		},
		"rootView": {
			"viewName": "sap.ui.mdc.demokit.sample.Chart.ChartJS.view.Demo",
			"async": false,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.114.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.mdc": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"products": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "Products"
			},
			"propertyinfos": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "PropertyInfos"
			}
		}
	}
}