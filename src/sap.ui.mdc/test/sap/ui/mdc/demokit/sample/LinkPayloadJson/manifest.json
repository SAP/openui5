{
	"_version": "2.0.1",
	"sap.app": {
		"id": "mdc.sample",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "MDC Sample",
		"description": "MDC Sample Application",
		"dataSources": {
			"products": {
				"uri": "model/products.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"config": {
			"sample": {
				"files": [
					"manifest.json",
					"controller/Products.controller.js",
					"delegate/JSONLinkDelegate.js",
					"model/products.json",
					"view/Products.view.xml"
				]
			}
		},
		"flexEnabled": false,
		"resourceRoots": {
			"mdc.sample": "./"
		},
		"dependencies": {
			"minUI5Version": "1.120.0",
			"libs": {
				"sap.f": {},
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.fl": {},
				"sap.ui.mdc": {},
				"sap.ui.table": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"handleValidation": true,
		"models": {
			"products": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "products"
			}
		},
		"rootView": {
			"viewName": "mdc.sample.view.Products",
			"id": "sample"
		}
	}
}