{
	"_version": "2.0.0",
	"sap.app": {
		"id": "sap.ui.mdc.demokit.sample.FieldCustomContent",
		"type": "application",
		"title": "Fields with custom content",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"DataSource": {
				"type": "JSON",
				"uri": "./model/data.json"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"config": {
			"sample": {
				"files": [
					"View.view.xml",
					"Controller.controller.js"
				]
			}
		},
		"resourceRoots": {
			"mdc.sample": "./"
		},
		"rootView": {
			"viewName": "mdc.sample.View",
			"id": "sample"
		},
		"dependencies": {
			"minUI5Version": "1.121.0",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.mdc": {},
				"sap.m": {}
			}
		},
		"handleValidation": true,
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"data": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "DataSource"
			}
		}
	}
}