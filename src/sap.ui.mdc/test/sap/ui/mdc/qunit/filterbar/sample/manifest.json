{
	"_version": "2.0.2",
	"sap.app": {
		"id": "sap.ui.mdc.filterbar.sample",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "CA-UI5-MDC",
		"title": "Application 'sample'"
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"id": "IDView",
			"viewName": "sap.ui.mdc.filterbar.sample.FilterBarTest",
			"height": "100%"
		},
		"handleValidation": true,
		"dependencies": {
			"minUI5Version": "${project.version}",
			"libs": {
				"sap.m": {
					"lazy": true
				},
				"sap.ui.layout": {
					"lazy": true
				},
				"sap.ui.fl": {
					"lazy": false
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			}
		}
	}
}