{
	"_version": "0.1.0",
	"sap.app": {
		"_version": "0.1.0",
		"id": "sap.ui.v4demo",
		"type": "application",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"default": {
				"uri": "/tenant(ValueHelpOpaTestApplication)/catalog/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "0.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.7.0",
		"rootView": {
			"viewName": "sap.ui.v4demo.view.App",
			"height": "100%",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.70",
			"libs": {
				"sap.ui.core" : {
					"minVersion" : "1.58.0",
					"lazy": true
				},
				"sap.m": {
					"lazy": true
				},
				"sap.ui.layout": {
					"lazy": true
				},
				"sap.ui.fl": {
					"lazy": false
				},
				"sap.ui.mdc": {
					"lazy": true
				}
			}
		},
		"autoPrefixId": true,
		"models": {
			"": {
				"dataSource": "default",
				"preload": true,
				"settings": {
					"autoExpandSelect": true,
					"operationMode": "Server",
					"synchronizationMode": "None",
					"updateGroupId": "booksGroup"
				},
				"type": "sap.ui.model.odata.v4.ODataModel"
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	}
}
