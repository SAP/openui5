<mvc:View height="100%" controllerName="sap.ui.mdc.sample.field.filterField.Test"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:m="sap.m"
	xmlns:mdc="sap.ui.mdc"
	xmlns:mdcField="sap.ui.mdc.field"
	xmlns:mdcValueHelp="sap.ui.mdc.valuehelp"
	xmlns:mdcValueHelpContent="sap.ui.mdc.valuehelp.content"
	xmlns:vhfb="sap.ui.mdc.filterbar.vh"
	xmlns:l="sap.ui.layout"
	xmlns:form="sap.ui.layout.form"
	xmlns:c="sap.ui.core"
	xmlns:legend="sap.ui.core.feature.legend"
	xmlns:custom="sap.ui.mdc.sample.field.lib">

	<m:App id="MyApp" defaultTransitionName="show">
		<m:Page id="P0" title="FilterField" titleLevel="H1">
			<m:ScrollContainer vertical="true" height="67%">
				<form:SimpleForm id="Form1" layout="ColumnLayout" editable="true"
					columnsXL="4" columnsL="3" columnsM="2">
					<c:Title text="FilterFields"/>

					<m:Label text="single value field (ProductId)"/>
					<mdc:FilterField id="FF1"
						conditions="{cm>/conditions/ProductId}"
						propertyKey="ProductId"
						display="Description"
						maxConditions="1"
						placeholder="{/#Product/ProductId/@sap:label}"
						label="{/#Product/ProductId/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange"
						valueHelp="VH1">
						<mdc:dependents>
							<mdc:ValueHelp id="VH1" delegate='\{"name": "sap/ui/mdc/sample/field/ValueHelpODataV2.delegate", "payload": \{"searchKeys":["ProductId","Name"]\}\}'>
								<mdc:typeahead>
									<mdcValueHelp:Popover id="VH1-Pop">
										<mdcValueHelpContent:MTable id="VH1-Pop-MTable"
											keyPath="ProductId"
											descriptionPath="Name"
											caseSensitive="true"
											useAsValueHelp="false">
											<m:Table id="VH1-Pop-Table" items="{path: '/ProductCollection'}" width="26rem">
												<m:columns>
													<m:Column>
														<m:Label text="{/#Product/ProductId/@sap:label}"/>
													</m:Column>
													<m:Column width="10rem">
														<m:Label text="{/#Product/Name/@sap:label}"/>
													</m:Column>
													<m:Column width="2rem">
														<m:Label text="Icon"/>
													</m:Column>
												</m:columns>
												<m:items>
													<m:ColumnListItem type="Active">
														<m:Text text="{ProductId}"/>
														<m:Text text="{Name}"/>
														<m:Image src="{= '../../../../../../' + ${ProductPicUrl}}" height="1rem"/>
													</m:ColumnListItem>
												</m:items>
											</m:Table>
										</mdcValueHelpContent:MTable>
									</mdcValueHelp:Popover>
								</mdc:typeahead>
								<mdc:dialog>
									<mdcValueHelp:Dialog id="VH1-Dialog" title="Product">
										<mdcValueHelpContent:MTable id="VH1-Dialog-MTable"
											keyPath="ProductId"
											descriptionPath="Name"
											caseSensitive="true"
											useAsValueHelp="false">
											<m:Table id="VH1-Dialog-Table" items="{path: '/ProductCollection'}">
												<m:columns>
													<m:Column>
														<m:Label text="{/#Product/ProductId/@sap:label}"/>
													</m:Column>
													<m:Column width="10rem">
														<m:Label text="{/#Product/Name/@sap:label}"/>
													</m:Column>
													<m:Column width="2rem">
														<m:Label text="Icon"/>
													</m:Column>
												</m:columns>
												<m:items>
													<m:ColumnListItem type="Active">
														<m:Text text="{ProductId}"/>
														<m:Text text="{Name}"/>
														<m:Image src="{= '../../../../../../' + ${ProductPicUrl}}" height="1rem"/>
													</m:ColumnListItem>
												</m:items>
											</m:Table>
										</mdcValueHelpContent:MTable>
									</mdcValueHelp:Dialog>
								</mdc:dialog>
							</mdc:ValueHelp>
						</mdc:dependents>
					</mdc:FilterField>

					<m:Label text="single value field in display mode (ProductId)"/>
					<mdc:FilterField id="FF2"
						conditions="{cm>/conditions/ProductId}"
						propertyKey="ProductId"
						display="Description"
						maxConditions="1"
						editMode="Display"
						placeholder="{/#Product/ProductId/@sap:label}"
						label="{/#Product/ProductId/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange"
						valueHelp="VH1">
					</mdc:FilterField>

					<m:Label text="multi value field (Name)"/>
					<mdc:FilterField
						id="FF3"
						conditions="{cm>/conditions/Name}"
						propertyKey="Name"
						maxConditions="10"
						placeholder="{/#Product/Name/@sap:label}"
						label="{/#Product/Name/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange"
						valueHelp="VH1">
					</mdc:FilterField>

					<m:Label text="multi value field in display mode (Name)"/>
					<mdc:FilterField id="FF4"
						conditions="{cm>/conditions/Name}"
						propertyKey="Name"
						maxConditions="10"
						editMode="Display"
						placeholder="{/#Product/Name/@sap:label}"
						label="{/#Product/Name/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange">
					</mdc:FilterField>

					<m:Label text="multi value field (Description)"/>
					<mdc:FilterField id="FF5"
						conditions="{cm>/conditions/Description}"
						propertyKey="Description"
						dataType="sap.ui.model.odata.type.String"
						dataTypeConstraints='\{"nullable": false\}'
						delegate='\{"name": "delegates/odata/v4/FieldBaseDelegate"\}'
						maxConditions="3"
						placeholder="{/#Product/Description/@sap:label}"
						label="{/#Product/Description/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange">
					</mdc:FilterField>

					<m:Label text="multi value field in display mode (Description)"/>
					<mdc:FilterField id="FF6"
						conditions="{cm>/conditions/Description}"
						propertyKey="Description"
						maxConditions="3"
						editMode="Display"
						placeholder="{/#Product/Description/@sap:label}"
						label="{/#Product/Description/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange">
					</mdc:FilterField>

					<m:Label text="single value field (DateTime) only equals"/>
					<mdc:FilterField id="FF7"
						conditions="{cm>/conditions/DateTime}"
						propertyKey="DateTime"
						dataType="sap.ui.model.odata.type.DateTime"
						dataTypeFormatOptions='\{"style": "long"\}'
						delegate='\{"name": "sap/ui/mdc/sample/field/FieldBaseODataV2.delegate"\}'
						display="Value"
						maxConditions="1"
						operators="EQ"
						placeholder="{/#Product/DateTime/@sap:label}"
						label="{/#Product/DateTime/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange">
					</mdc:FilterField>

					<m:Label text="single value field (DateTime) with DynamicDateRange"/>
					<mdc:FilterField id="FF7d"
						conditions="{cm>/conditions/DateTime}"
						propertyKey="DateTime"
						dataType="sap.ui.model.odata.type.DateTime"
						dataTypeFormatOptions='\{"style": "short"\}'
						delegate='\{"name": "sap/ui/mdc/sample/field/FieldBaseODataV2.delegate"\}'
						display="Value"
						maxConditions="1"
						placeholder="{/#Product/DateTime/@sap:label}"
						label="{/#Product/DateTime/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange">
					</mdc:FilterField>

					<m:Label text="single value field in display mode (DateTime)"/>
					<mdc:FilterField id="FF8"
						conditions="{cm>/conditions/DateTime}"
						propertyKey="DateTime"
						dataType="sap.ui.model.odata.type.DateTime"
						dataTypeFormatOptions='\{"style": "medium"\}'
						delegate='\{"name": "sap/ui/mdc/sample/field/FieldBaseODataV2.delegate"\}'
						maxConditions="1"
						editMode="Display"
						placeholder="{/#Product/DateTime/@sap:label}"
						label="{/#Product/DateTime/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange">
					</mdc:FilterField>

					<m:Label text="single value field (Date) all operators"/>
					<mdc:FilterField id="FF7a"
						conditions="{cm>/conditions/Date}"
						propertyKey="Date"
						dataType="sap.ui.model.odata.type.DateTime"
						dataTypeFormatOptions='\{"style": "long"\}'
						dataTypeConstraints='\{"displayFormat": "Date"\}'
						delegate='\{"name": "sap/ui/mdc/sample/field/FieldBaseODataV2.delegate"\}'
						display="Value"
						maxConditions="1"
						placeholder="{/#Product/Date/@sap:label}"
						label="{/#Product/Date/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange"
						valueHelp="VH-Conditions">
					</mdc:FilterField>

					<m:Label text="single value field (Date) using DynamicDateRange"/>
					<mdc:FilterField id="FF7a-DDR"
						conditions="{cm>/conditions/Date}"
						propertyKey="Date"
						dataType="sap.ui.model.odata.type.DateTime"
						dataTypeFormatOptions='\{"style": "long"\}'
						dataTypeConstraints='\{"displayFormat": "Date"\}'
						delegate='\{"name": "sap/ui/mdc/sample/field/FieldBaseODataV2.delegate"\}'
						display="Value"
						maxConditions="1"
						placeholder="{/#Product/Date/@sap:label}"
						label="{/#Product/Date/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange">
					</mdc:FilterField>

					<m:Label text="single value field (Date) only between"/>
					<mdc:FilterField id="FF7b"
						conditions="{cm>/conditions/Date}"
						propertyKey="Date"
						dataType="sap.ui.model.odata.type.DateTime"
						dataTypeFormatOptions='\{"style": "short"\}'
						dataTypeConstraints='\{"displayFormat": "Date"\}'
						delegate='\{"name": "sap/ui/mdc/sample/field/FieldBaseODataV2.delegate"\}'
						display="Value"
						maxConditions="1"
						operators="BT"
						placeholder="{/#Product/Date/@sap:label}"
						label="{/#Product/Date/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange">
					</mdc:FilterField>

					<m:Label text="multi value field (Date) only equal"/>
					<mdc:FilterField id="FF7c"
						conditions="{cm>/conditions/Date}"
						propertyKey="Date"
						dataType="sap.ui.model.odata.type.DateTime"
						dataTypeFormatOptions='\{"style": "short"\}'
						dataTypeConstraints='\{"displayFormat": "Date"\}'
						delegate='\{"name": "sap/ui/mdc/sample/field/FieldBaseODataV2.delegate"\}'
						display="Value"
						maxConditions="-1"
						operators="EQ"
						placeholder="{/#Product/Date/@sap:label}"
						label="{/#Product/Date/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange"
						valueHelp="VH-Conditions">
					</mdc:FilterField>

					<m:Label text="single value field (Status)"/>
					<mdc:FilterField id="FF9"
						conditions="{cm>/conditions/Status}"
						propertyKey="Status"
						display="Description"
						maxConditions="1"
						placeholder="{/#Product/Status/@sap:label}"
						label="{/#Product/Status/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange"
						valueHelp="VH-Status">
						<mdc:dependents>
							<mdc:ValueHelp id="VH-Status" delegate='\{"name": "sap/ui/mdc/sample/field/ValueHelpODataV2.delegate"\}'>
								<mdc:typeahead>
									<mdcValueHelp:Popover id="VH-Status-Pop">
										<mdcValueHelpContent:FixedList id="VH-Status-FL" useFirstMatch="false" items="{path: '/StatusCollection'}">
											<mdcValueHelpContent:FixedListItem key="{StatusId}" text="{Name}"/>
										</mdcValueHelpContent:FixedList>
									</mdcValueHelp:Popover>
								</mdc:typeahead>
							</mdc:ValueHelp>
						</mdc:dependents>
					</mdc:FilterField>

					<m:Label text="Select-like single value field (Status)"/>
					<mdc:FilterField id="FF17"
						conditions="{cm>/conditions/Status}"
						propertyKey="Status"
						display="Description"
						maxConditions="1"
						placeholder="{/#Product/Status/@sap:label}"
						label="{/#Product/Status/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange"
						valueHelp="VH-Status2">
						<mdc:dependents>
							<mdc:ValueHelp id="VH-Status2" delegate='\{"name": "sap/ui/mdc/sample/field/ValueHelpODataV2.delegate"\}'>
								<mdc:typeahead>
									<mdcValueHelp:Popover id="VH-Status2-Pop">
										<mdcValueHelpContent:FixedList id="VH-Status2-FL" useFirstMatch="true" filterList="false" items="{path: '/StatusCollection'}">
											<mdcValueHelpContent:FixedListItem key="{StatusId}" text="{Name}"/>
										</mdcValueHelpContent:FixedList>
									</mdcValueHelp:Popover>
								</mdc:typeahead>
							</mdc:ValueHelp>
						</mdc:dependents>
					</mdc:FilterField>

					<m:Label text="single value field in display mode  (Status)"/>
					<mdc:FilterField id="FF10"
						conditions="{cm>/conditions/Status}"
						propertyKey="Status"
						maxConditions="1"
						editMode="Display"
						placeholder="{/#Product/Status/@sap:label}"
						label="{/#Product/Status/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange">
					</mdc:FilterField>

					<m:Label text="multi (2) value field (Quantity)"/>
					<mdc:FilterField id="FF11"
						conditions="{cm>/conditions/Quantity}"
						propertyKey="Quantity"
						dataType="Edm.Int32"
						dataTypeConstraints='\{"nullable": false\}'
						delegate='\{"name": "sap/ui/mdc/sample/field/FieldBaseODataV2.delegate"\}'
						display="Value"
						maxConditions="2"
						placeholder="{/#Product/Quantity/@sap:label}"
						label="{/#Product/Quantity/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange"
						valueHelp="VH-Conditions">
					</mdc:FilterField>

					<m:Label text="multi value field in display mode (Quantity)"/>
					<mdc:FilterField id="FF12"
						conditions="{cm>/conditions/Quantity}"
						propertyKey="Quantity"
						dataType="Edm.Int32"
						delegate='\{"name": "sap/ui/mdc/sample/field/FieldBaseODataV2.delegate"\}'
						maxConditions="2"
						editMode="Display"
						placeholder="{/#Product/Quantity/@sap:label}"
						label="{/#Product/Quantity/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange">
					</mdc:FilterField>

					<m:Label text="Custom field"/>
					<mdc:FilterField id="FF13"
						conditions="{cm>/conditions/Status}"
						propertyKey="Status"
						display="Description"
						maxConditions="1"
						placeholder="{/#Product/Status/@sap:label}"
						label="{/#Product/Status/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange">
						<custom:CustomFilter id="FF13-Custom"
							conditions="{$field>/conditions}"
							maxConditions="{$field>/maxConditions}"
						/>
					</mdc:FilterField>

					<m:Label text="A UoM field"/>
					<mdc:FilterField id="FF15"
						conditions="{cm>/conditions/WeightMeasure,WeightUnit}"
						propertyKey="WeightMeasure,WeightUnit"
						dataType="sap.ui.model.type.Unit"
						dataTypeConstraints='\{"minimum": 0\}'
						display="Description"
						editMode="Editable"
						placeholder="{/#Product/WeightMeasure/@sap:label}"
						label="{/#Product/WeightMeasure/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange"
						valueHelp="VH-WeightUnit">
						<mdc:dependents>
							<mdc:ValueHelp id="VH-WeightUnit" delegate='\{"name": "sap/ui/mdc/sample/field/ValueHelpODataV2.delegate"\}'>
								<mdc:typeahead>
									<mdcValueHelp:Popover id="VH-WeightUnit-Pop">
										<mdcValueHelpContent:FixedList id="VH-WeightUnit-FL" useFirstMatch="false" items="{/weightUnits}">
											<mdcValueHelpContent:FixedListItem key="{id}" text="{unit}" additionalText="{text}"/>
										</mdcValueHelpContent:FixedList>
									</mdcValueHelp:Popover>
								</mdc:typeahead>
							</mdc:ValueHelp>
						</mdc:dependents>
					</mdc:FilterField>

					<m:Label text="A UoM field in display mode"/>
					<mdc:FilterField id="FF16"
						conditions="{cm>/conditions/WeightMeasure,WeightUnit}"
						propertyKey="WeightMeasure,WeightUnit"
						dataType="sap.ui.model.type.Unit"
						display="Value"
						editMode="Display"
						placeholder="{/#Product/WeightMeasure/@sap:label}"
						label="{/#Product/WeightMeasure/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange">
					</mdc:FilterField>

					<m:Label text="Material (empty key)"/>
					<mdc:FilterField id="FF_Material"
						conditions="{cm>/conditions/MatrId}"
						propertyKey="MatrId"
						dataType="sap.ui.model.type.String"
						dataTypeConstraints='\{"maxLength": 10, "search": "^(|[0-9])+$", "nullable": false\}'
						dataTypeFormatOptions='\{"parseKeepsEmptyString": true\}'
						display="ValueDescription"
						placeholder="{/#Product/MatrId/@sap:label}"
						label="{/#Product/MatrId/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange"
						valueHelp="VH-Material">
						<mdc:dependents>
							<mdc:ValueHelp id="VH-Material" delegate='\{"name": "sap/ui/mdc/sample/field/ValueHelpODataV2.delegate", "payload": \{"searchKeys":["MatrId","Text"]\}\}'>
								<mdc:typeahead>
									<mdcValueHelp:Popover id="VH-Material-Pop">
										<mdcValueHelpContent:MTable id="VH-Material-Pop-MTable"
											keyPath="MatrId"
											descriptionPath="Text"
											caseSensitive="true"
											useAsValueHelp="false">
											<m:Table id="VH-Material-Pop-Table" items="{path: '/MaterialCollection'}" width="26rem">
												<m:columns>
													<m:Column>
														<m:Label text="id"/>
													</m:Column>
													<m:Column>
														<m:Label text="Desciption"/>
													</m:Column>
												</m:columns>
												<m:items>
													<m:ColumnListItem type="Active">
														<m:Text text="{MatrId}"/>
														<m:Text text="{Text}"/>
													</m:ColumnListItem>
												</m:items>
											</m:Table>
										</mdcValueHelpContent:MTable>
									</mdcValueHelp:Popover>
								</mdc:typeahead>
								<mdc:dialog>
									<mdcValueHelp:Dialog id="VH-Material-Dialog" title="Material">
										<mdcValueHelpContent:MTable id="VH-Material-Dialog-MTable"
											keyPath="MatrId"
											descriptionPath="Text"
											caseSensitive="true"
											useAsValueHelp="false">
											<m:Table id="VH-Material-Dialog-Table" items="{path: '/MaterialCollection'}">
												<m:columns>
													<m:Column>
														<m:Label text="id"/>
													</m:Column>
													<m:Column>
														<m:Label text="Desciption"/>
													</m:Column>
												</m:columns>
												<m:items>
													<m:ColumnListItem type="Active">
														<m:Text text="{MatrId}"/>
														<m:Text text="{Text}"/>
													</m:ColumnListItem>
												</m:items>
											</m:Table>
										</mdcValueHelpContent:MTable>
									</mdcValueHelp:Dialog>
								</mdc:dialog>
							</mdc:ValueHelp>
						</mdc:dependents>
					</mdc:FilterField>

					<m:Label text="Search"/>
					<mdc:FilterField id="FF-Search"
						conditions="{cm>/conditions/$search}"
						propertyKey="$search"
						maxConditions="1"
						dataType="sap.ui.model.type.String"
						display="Value"
						editMode="Editable"
						placeholder="My Search"
						label="My Search"
						change="handleChange"
						liveChange="handleLiveChange">
					</mdc:FilterField>

					<m:Label text="type date as additionalValue"/>
					<mdc:FilterField id="FF-DateDescription"
						conditions="{cm>/conditions/Name}"
						delegate='\{"name": "sap/ui/mdc/sample/field/FieldBaseODataV2.delegate"\}'
						dataType="sap.ui.model.type.String"
						dataTypeConstraints='\{"maxLength": 30\}'
						additionalDataType='\{"name": "sap.ui.model.odata.type.DateTime", "formatOptions": \{"style": "long"\}, "constraints": \{"displayFormat": "Date"\}\}'
						propertyKey="Name"
						display="ValueDescription"
						maxConditions="-1"
						placeholder="{/#Product/Name/@sap:label}"
						label="{/#Product/Name/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange"
						valueHelp="VH-DateDescription">
						<mdc:dependents>
							<mdc:ValueHelp id="VH-DateDescription" delegate='\{"name": "sap/ui/mdc/sample/field/ValueHelpODataV2.delegate"\}'>
								<mdc:typeahead>
									<mdcValueHelp:Popover id="VH-DateDescription-Pop">
										<mdcValueHelpContent:FixedList id="VH-DateDescription-FL" useFirstMatch="true" items="{path: '/ProductCollection'}" >
											<mdcValueHelpContent:FixedListItem 
												text="{path: 'Date', type:'sap.ui.model.odata.type.DateTime', formatOptions: {style: 'long'}, constraints: {displayFormat: 'Date'}}" 
												additionalText="{path: 'Name', type: 'sap.ui.model.type.String', constraints: {maxLength: 30}}" 
												key="{path: 'Name', type: 'sap.ui.model.type.String', constraints: {maxLength: 30}}"/>
										</mdcValueHelpContent:FixedList>
									</mdcValueHelp:Popover>
								</mdc:typeahead>
							</mdc:ValueHelp>
						</mdc:dependents>
					</mdc:FilterField>

					<c:Title text="In/Out Example"/>
					<m:Label text="{/#Product/CountryId/@sap:label}"/>
					<mdc:FilterField
						id="IOFFCountry"
						conditions="{cm>/conditions/CountryId}"
						propertyKey="CountryId"
						operators="EQ,NE,EUROPE"
						dataType="sap.ui.model.type.String"
						dataTypeConstraints='\{"maxLength": 3\}'
						display="ValueDescription"
						label="{/#Product/CountryId/@sap:label}"
						change="handleChange"
						liveChange="handleLiveChange"
						valueHelp="VH-Country">
						<mdc:dependents>
							<mdc:ValueHelp id="VH-Country" delegate='\{"name": "sap/ui/mdc/sample/field/ValueHelpODataV2.delegate", "payload": \{"searchKeys":["countryId","text"]\}\}'>
								<mdc:typeahead>
									<mdcValueHelp:Popover id="VH-Country-Pop">
										<mdcValueHelpContent:MTable id="VH-Country-Pop-MTable"
											keyPath="countryId"
											descriptionPath="text"
											caseSensitive="true"
											useAsValueHelp="false">
											<m:Table id="VH-Country-Pop-Table" items="{path: '/CountryCollection', suspended: true}" width="26rem">
												<m:columns>
													<m:Column>
														<m:Label text="{/#Country/countryId/@sap:label}"/>
													</m:Column>
													<m:Column>
														<m:Label text="{/#Country/text/@sap:label}"/>
													</m:Column>
												</m:columns>
												<m:items>
													<m:ColumnListItem type="Active">
