<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<MessagePopover
		items="{message>/}"
		initiallyExpanded="true"
		activeTitlePress="handleMessagePress">
		<MessageItem
			type="{message>type}"
			title="{message>message}"
			subtitle="{message>additionalText}"
			description="{message>description}"
			activeTitle="true">
		</MessageItem>
	</MessagePopover>
</core:FragmentDefinition>
